<RecipeDefs>

  <RecipeDef Name="AmmoRecipePistol" Abstract="true">
    <workSpeedStat>SmithingSpeed</workSpeedStat>
	<effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>5000</workAmount>
    <workSkill>Crafting</workSkill>
    <targetCountAdjustment>50</targetCountAdjustment>
	<recipeUsers>
	  <!-- Need an empty list here or the ammo injector will throw a null ref exception -->
	</recipeUsers>
  </RecipeDef>

  <!-- ==================== .40 Rimfire ========================== -->

  <RecipeDef ParentName="AmmoRecipePistol">
    <defName>MakeAmmo_40Rimfire_FMJ</defName>
    <label>make .40 Rimfire (FMJ) cartridge x50</label>
    <description>Craft 250 .40 Rimfire (FMJ) cartridges.</description>
    <jobString>Making .40 Rimfire (FMJ) cartridges.</jobString>
    <ingredients>
      <li>
        <filter>
					<categories>
						<li>Metallic</li>
                    </categories>
        </filter>
        <count>3</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Powder</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
                    </categories>
      <thingDefs>
        <li>Powder</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_40Rimfire_FMJ>50</Ammo_40Rimfire_FMJ>
    </products>
		<skillRequirements>
			<li>
				<skill>Crafting</skill>
				<minLevel>2</minLevel>
			</li>
		</skillRequirements>
    <workSkill>Crafting</workSkill>
    <workSkillLearnFactor>0.8</workSkillLearnFactor>
  </RecipeDef>


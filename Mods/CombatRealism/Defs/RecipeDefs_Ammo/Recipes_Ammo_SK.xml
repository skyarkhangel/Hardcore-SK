<RecipeDefs>


  <!-- ==================== bolts ========================== -->

  <RecipeDef ParentName="AmmoRecipeNeolithicBase">
    <defName>MakeAmmo_Bolt</defName>
    <label>make bolts x10</label>
    <description>Craft 10 bolts.</description>
    <jobString>Making bolts.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>WoodLog</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_Bolt>8</Ammo_Bolt>
    </products>
	<recipeUsers>
      <li>ElectricSmithy</li>
      <li>FueledSmithy</li>
      <li>CraftingSpot</li>
	</recipeUsers>
  </RecipeDef>
  

  <!-- ==================== 9x39mm Soviet sniper standart ========================== -->

  <RecipeDef ParentName="AmmoRecipeBase">
    <defName>MakeAmmo_9x39Soviet</defName>
    <label>make 9x39mm Soviet sniper standart cartridge x500</label>
    <description>Craft 500 9x39mm Soviet sniper standart cartridges.</description>
    <jobString>Making 9x39mm Soviet sniper standart cartridges.</jobString>
    <ingredients>
      <li>
        <filter>
					<categories>
						<li>SLDBar</li>
                    </categories>
        </filter>
        <count>7</count>
      </li>
      <li>
        <filter>
					<categories>
						<li>USLDBar</li>
                    </categories>
        </filter>
        <count>4</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Powder</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
                    </categories>
      <thingDefs>
        <li>Powder</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
		<Ammo_9x39Soviet>200</Ammo_9x39Soviet>
    </products>
		<skillRequirements>
			<li>
				<skill>Crafting</skill>
				<minLevel>8</minLevel>
			</li>
		</skillRequirements>
    <workSkill>Crafting</workSkill>
    <workSkillLearnFactor>0.8</workSkillLearnFactor>
  </RecipeDef>
  
  

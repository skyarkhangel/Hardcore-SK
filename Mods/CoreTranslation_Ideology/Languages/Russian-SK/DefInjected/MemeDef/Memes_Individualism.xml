    <li>founder_threw(founder_gender==Female)->бросилась</li>
    <li>founder_united(founder_gender==Male)->сплотил</li>
    <li>founder_united(founder_gender==Female)->сплотила</li>
    <li>founder_punished(founder_gender==Male)->наказал</li>
    <li>founder_punished(founder_gender==Female)->наказала</li>
    <li>founder_killed(founder_gender==Male)->убил</li>

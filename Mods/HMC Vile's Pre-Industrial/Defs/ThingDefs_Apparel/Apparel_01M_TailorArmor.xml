			<texPath>Things/Apparel/MedievalHeadNew/FeralHood/FeralHood</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<costStuffCount>35</costStuffCount>
		<stuffCategories>
			<li>Fabric</li>
		</stuffCategories>
		<costList>
			<Wire>20</Wire>
		</costList>
		<statBases>
			<WorkToMake>5000</WorkToMake>
			<Mass>1</Mass>
			<Bulk>1.5</Bulk>
			<WornBulk>0</WornBulk>
			<EquipDelay>2.1</EquipDelay>
			<Insulation_Cold>5</Insulation_Cold>
			<Insulation_Heat>5</Insulation_Heat>
			<StuffEffectMultiplierInsulation_Cold>0.4</StuffEffectMultiplierInsulation_Cold>

<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>	
			<li Class="PatchOperationReplace"> 
				<xpath>/ThingDefs/ThingDef[DefName = "TableAnimalProsthetics"]/CostList</xpath>
				<value>
					<CostList>
						<Component>10</Component>
					</CostList>
				</value>
			</li>			
			<li Class="PatchOperationAdd"> 
				<xpath>/ThingDefs/ThingDef[DefName = "TableAnimalProsthetics"]</xpath>
				<value>
					<stuffCategories>
						<li>Metallic</li>
					</stuffCategories>
					<costStuffCount>100</costStuffCount>
				</value>
			</li>	
	    </operations>
  </Operation>
</Patch>
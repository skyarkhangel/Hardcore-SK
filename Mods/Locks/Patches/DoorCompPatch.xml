<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Core SK</li>
		</mods>
		<success>Always</success>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef[defName = "MDoor"]/comps</xpath>
					<success>Always</success>
					<nomatch Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName = "MDoor"]</xpath>
						<value>
							<comps />
						</value>
					</nomatch>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName = "MDoor"]/comps</xpath>
					<value>
						<li Class = "Locks.CompProperties_Lock">
							<compClass>Locks.CompLock</compClass>
						</li>
					</value>
				</li>
				<li Class="PatchOperationConditional">
					<xpath>Defs/ThingDef[defName = "MDoor"]/inspectorTabs</xpath>
					<success>Always</success>
					<nomatch Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName = "MDoor"]</xpath>
						<value>
							<inspectorTabs />
						</value>
					</nomatch>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName = "MDoor"]/inspectorTabs</xpath>
					<value>
						<li>Locks.ITab_Lock</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	

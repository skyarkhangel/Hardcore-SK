<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BenchBase">
    <defName>Blackboard</defName>
    <label>blackboard</label>
    <thingClass>Blackboard.Building_Blackboard</thingClass>
    <description>A blackboard for teaching.</description>
    <graphicData>
      <texPath>Things/Building/Production/Building_Blackboard</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3.5,1.5)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <costList>
     <ComponentIndustrial>5</ComponentIndustrial>
    </costList>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
    </stuffCategories>
    <costStuffCount>80</costStuffCount>
    <altitudeLayer>Building</altitudeLayer>
    <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>1600</WorkToBuild>
      <MaxHitPoints>180</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <Cleanliness>-5</Cleanliness>
    </statBases>
    <size>(3,1)</size>
    <designationCategory>Accessories</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
    <comps>
	  <li Class="CompProperties_Facility">
        <statOffsets>
          <WorkTableWorkSpeedFactor>1.0</WorkTableWorkSpeedFactor>
        </statOffsets>
        <maxSimultaneous>1</maxSimultaneous>
        <maxDistance>11.0</maxDistance>
      </li>
    </comps>
    <designationHotKey></designationHotKey>
    <placeWorkers>
		<li>PlaceWorker_ShowFacilitiesConnections</li>
    </placeWorkers>
  </ThingDef>

</Defs>
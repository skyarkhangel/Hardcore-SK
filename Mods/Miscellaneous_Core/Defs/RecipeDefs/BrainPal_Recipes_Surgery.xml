		<workAmount>2000</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ElectronicTacticalImplant</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
		<categories>
				<li>Medicine</li>
			</categories>
			<thingDefs>
				<li>ElectronicTacticalImplant</li>

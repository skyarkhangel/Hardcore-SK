<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ASARI -->
	<AlienRace.ThingDef_AlienRace ParentName="BaseHumanlikePawn">
		<defName>Alien_Asari</defName>
		<label>Asari</label>
		<thingClass>SK.Asari</thingClass>
		<description>A mono-gender race, the asari are distinctly feminine in appearance and possess maternal instincts. Their unique physiology, expressed in a millennium-long lifespan and the ability to reproduce with a partner of any gender or species, gives them a conservative yet convivial attitude toward other races.</description>
		<alienRace>
			<generalSettings>
				<humanRecipeImport>true</humanRecipeImport>
				<maleGenderProbability>0.00001</maleGenderProbability>
				<immuneToAge>true</immuneToAge>
				<forcedRaceTraitEntries>
					<li>
						<defName>Beauty</defName>
						<degree>2</degree>
						<chance>20</chance>
					</li>
					<li>
						<defName>Beauty</defName>
						<degree>1</degree>
						<chance>65</chance>
					</li>
					<li>
						<defName>NaturalMood</defName>
						<degree>1</degree>
						<chance>20</chance>
					</li>
					<li>
						<defName>Xenophobia</defName>
						<degree>1</degree>
						<chance>0</chance>
					</li>
					<li>
						<defName>Xenophobia</defName>
						<degree>-1</degree>
						<chance>30</chance>
					</li>
				</forcedRaceTraitEntries>
				<alienPartGenerator>
					<aliencrowntypes>
						<li>Average_Normal</li>
					</aliencrowntypes>
					<alienbodytypes>
						<li>Female</li>
					</alienbodytypes>
					<colorChannels>
						<li>
							<name>skin</name>
							<first Class="ColorGenerator_Options">
								<options>
									<li>
										<weight>7</weight>
										<only>(0.243,0.333,0.561,1)</only>
									</li>
									<li>
										<weight>3</weight>
										<only>(0.310,0.373,0.533,1)</only>
									</li>
									<li>
										<weight>2</weight>
										<only>(0.231,0.420,0.467,1)</only>
									</li>
									<li>
										<weight>1</weight>
										<only>(0.133,0.431,0.706,1)</only>
									</li>
								</options>
							</first>
						</li>
					</colorChannels>
					<customDrawSize>(1.0, 1.0)</customDrawSize>
					<customPortraitDrawSize>(1.0, 1.0)</customPortraitDrawSize>
				</alienPartGenerator>
			</generalSettings>
			<graphicPaths>
				<li>
					<head>Things/Pawn/Humanlike/AsariHeads/</head>
				</li>
			</graphicPaths>
			<hairSettings>
				<hasHair>false</hasHair>
				<getsGreyAt>950</getsGreyAt>
			</hairSettings>
			<thoughtSettings>
				<cannotReceiveThoughts>
					<li>CabinFever</li>
				</cannotReceiveThoughts>
				<butcherThoughtSpecific>
					<li>
						<raceList>
							<li>Human</li>
							<li>Alien_Nova</li>
							<li>Alien_Dova</li>
							<li>Norbal</li>
							<li>Alien_Orassan</li>
							<li>Alien_Asari</li>
						</raceList>
						<thought>ButcheredHuman</thought>
						<knowThought>KnowButcheredHuman</knowThought>
					</li>
				</butcherThoughtSpecific>
				<ateThoughtSpecific>
					<li>
						<raceList>
							<li>Human</li>
							<li>Alien_Nova</li>
							<li>Alien_Dova</li>
							<li>Norbal</li>
							<li>Alien_Orassan</li>
							<li>Alien_Asari</li>
						</raceList>
						<thought>AteHumanlikeMeatDirect</thought>
						<ingredientThought>AteHumanIngredient</ingredientThought>
					</li>
					<li>
						<raceList>
							<li>Human</li>
							<li>Alien_Nova</li>
							<li>Alien_Dova</li>
							<li>Norbal</li>
							<li>Alien_Orassan</li>
							<li>Alien_Asari</li>
						</raceList>
						<thought>AteHumanlikeMeatDirectCannibal</thought>
						<ingredientThought>AteHumanlikeMeatAsIngredientCannibal</ingredientThought>
					</li>
				</ateThoughtSpecific>
			</thoughtSettings>			
		</alienRace>
		<statBases>
			<Mass>65</Mass>
			<PsychicSensitivity>1.5</PsychicSensitivity>
			<StuffPower_Armor_Heat>0.5</StuffPower_Armor_Heat>
			<ImmunityGainSpeed>1.2</ImmunityGainSpeed>
			<GlobalLearningFactor>1.2</GlobalLearningFactor>
			<ToxicSensitivity>0.75</ToxicSensitivity>
			<MarketValue>6000</MarketValue>
			<MoveSpeed>5.0</MoveSpeed>
			<Flammability>1</Flammability>
			<ComfyTemperatureMin>-7</ComfyTemperatureMin>
			<ComfyTemperatureMax>35</ComfyTemperatureMax>
			<LeatherAmount>30</LeatherAmount>
			<MeatAmount>90</MeatAmount>
			<MeleeDodgeChance>0.7</MeleeDodgeChance>
			<MeleeCritChance>0.3</MeleeCritChance>
			<MeleeParryChance>0.3</MeleeParryChance>
			<GermResistance>0.07</GermResistance>
			<GermContainment>0.08</GermContainment>
			<MeditationFocusGain MayRequire="Ludeon.RimWorld.Royalty">1</MeditationFocusGain>
		</statBases>
		<tools>
			<li Class="CombatExtended.ToolCE">
				<label>left fist</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>6</power>
				<cooldownTime>1.3</cooldownTime>
				<chanceFactor>0.5</chanceFactor>
				<linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
				<armorPenetrationBlunt>1.2</armorPenetrationBlunt>
			</li>
			<li Class="CombatExtended.ToolCE">
				<label>right fist</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>6</power>
				<cooldownTime>1.3</cooldownTime>
				<chanceFactor>0.5</chanceFactor>
				<linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
				<armorPenetrationBlunt>1.2</armorPenetrationBlunt>
			</li>
			<li Class="CombatExtended.ToolCE">
				<label>teeth</label>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>6</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.3</chanceFactor>
				<armorPenetrationSharp>0.6</armorPenetrationSharp>
				<armorPenetrationBlunt>1</armorPenetrationBlunt>
				<soundMeleeHit>Pawn_Melee_HumanBite_Hit</soundMeleeHit>
				<soundMeleeMiss>Pawn_Melee_HumanBite_Miss</soundMeleeMiss>
			</li>
			<li Class="CombatExtended.ToolCE">
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>6</power>
				<cooldownTime>2.75</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<chanceFactor>0.2</chanceFactor>
				<armorPenetrationBlunt>1.4</armorPenetrationBlunt>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
			</li>
		</tools>
		<tickerType>Normal</tickerType>
		<race>
			<useMeatFrom>Human</useMeatFrom>
			<leatherDef>Leather_Human</leatherDef>
			<lifeExpectancy>1200</lifeExpectancy>
			<bloodDef>Filth_AsariBlood</bloodDef>
			<baseHungerRate>0.75</baseHungerRate>
			<baseBodySize>1</baseBodySize>
			<baseHealthScale>1.15</baseHealthScale>
			<ageGenerationCurve>
				<points>
					<li>(14,0)</li>
					<li>(70,100)</li>
					<li>(650,100)</li>
					<li>(850,80)</li>
					<li>(950,65)</li>
					<li>(1050,40)</li>
					<li>(1200,20)</li>
					<li>(1300,0)</li>
				</points>
			</ageGenerationCurve>
		</race>
		<modExtensions>
			<li Class="CombatExtended.RacePropertiesExtensionCE">
				<bodyShape>Humanoid</bodyShape>
			</li>
		</modExtensions>
		<comps>
			<li>
				<compClass>CombatExtended.CompPawnGizmo</compClass>
			</li>
		</comps>
	</AlienRace.ThingDef_AlienRace>

</Defs>

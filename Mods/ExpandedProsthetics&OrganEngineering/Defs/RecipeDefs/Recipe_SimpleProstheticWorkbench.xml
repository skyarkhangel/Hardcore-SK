      <jobString>Crafting prosthesis.</jobString>
      <products>
         <SimpleProstheticLeg>1</SimpleProstheticLeg>
      </products>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>Steel</li>
               </thingDefs>
            </filter>
            <count>55</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Plasteel</li>
               </thingDefs>
            </filter>
            <count>25</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Components</li>
               </thingDefs>
            </filter>
            <count>3</count>

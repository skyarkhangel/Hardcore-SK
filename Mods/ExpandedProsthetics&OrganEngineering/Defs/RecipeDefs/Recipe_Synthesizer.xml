   <RecipeDef ParentName="RecipeSyntheticOrgan">
      <defName>CreateAIChip</defName>
      <label>Craft AI chip</label>
      <description>Creates an AI chip. Needs gold and avanced components for highest quality circuitry.</description>
      <jobString>Creating synthetic organ.</jobString>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>Gold</li>
               </thingDefs>
            </filter>
            <count>3</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Plasteel</li>
               </thingDefs>
            </filter>
            <count>25</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>AdvancedComponents</li>
               </thingDefs>
            </filter>
            <count>5</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <thingDefs>
            <li>Gold</li>
            <li>Plasteel</li>
            <li>AdvancedComponents</li>
         </thingDefs>
      </fixedIngredientFilter>
      <products>
         <AIChip>1</AIChip>
      </products>
   </RecipeDef>
   

<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<PawnKindDef class="AIPawn.PawnKindDef_AIPawn">
		<defName>AIPawn</defName>
		<label>Mobile AI</label>
		<race>AIPawn</race>
		<combatPower>999999</combatPower>
		<defaultFactionType>Colony</defaultFactionType>
		<baseRecruitDifficulty>5</baseRecruitDifficulty>
		<backstoryCategory>Spacer</backstoryCategory>
		<isFighter>false</isFighter>
		<lifeStages>
			<li>
				<bodyGraphicData>
					<texPath>Things/Pawns/Naked_Mia</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>3</drawSize>
					<shadowData>
						<volume>(0.4, 0.8, 0.4)</volume>
						<offset>(0,0,0)</offset>
					</shadowData>
				</bodyGraphicData>
			</li>
		</lifeStages>
		<weaponMoney>
			<min>5000</min>
			<max>5000</max>
		</weaponMoney>
		<weaponTags>
			<li>Gun</li>
		</weaponTags>
		<apparelTags>
			<li>Spacer</li>
		</apparelTags>
		<apparelMoney>
			<min>0</min>
			<max>0</max>
		</apparelMoney>
		<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
		<backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
		<forceNormalGearQuality>true</forceNormalGearQuality>
	</PawnKindDef>


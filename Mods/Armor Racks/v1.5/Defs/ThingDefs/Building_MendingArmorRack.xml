<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="ArmorRacks_BaseArmorRack">
        <defName>ArmorRacks_MendingArmorRack</defName>
        <label>mending armor rack</label>
        <description>Good for keeping sets of apparel conveniently organized. Mechanized for extra equipping efficiency. Mends stored things via power of mechanites.</description>
        <thingClass>ArmorRacks.Things.MendingArmorRack</thingClass>
        <graphicData>
            <texPath>ArmorRacks_MendingArmorRack</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(1,2)</drawSize>
        </graphicData>
        <statBases>
            <MaxHitPoints>180</MaxHitPoints>
            <Flammability>1.0</Flammability>
            <WorkToBuild>7500</WorkToBuild>
            <WorkTableWorkSpeedFactor>0.5</WorkTableWorkSpeedFactor>
            <Cleanliness>-5</Cleanliness>
            <Mass>25</Mass>
        </statBases>
        <size>(1,2)</size>
        <hasInteractionCell>True</hasInteractionCell>
        <interactionCellOffset>(0,0,2)</interactionCellOffset>
        <costList>
            <Steel>20</Steel>
            <Uranium>20</Uranium>
            <ComponentIndustrial>4</ComponentIndustrial>
            <ComponentSpacer>1</ComponentSpacer>
        </costList>
        <costStuffCount>80</costStuffCount>
        <stuffCategories>
            <li>Metallic</li>
        </stuffCategories>
        <defaultPlacingRot>South</defaultPlacingRot>
        <researchPrerequisites>
            <li>ArmorRacks_MendingArmorRackResearch</li>
        </researchPrerequisites>
        <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
        <comps>
            <li Class="CompProperties_Power">
                <compClass>CompPowerTrader</compClass>
                <basePowerConsumption>250</basePowerConsumption>
            </li>
            <li Class="CompProperties_Flickable"/>
        </comps>
        <tickerType>Rare</tickerType>
    </ThingDef>
</Defs>
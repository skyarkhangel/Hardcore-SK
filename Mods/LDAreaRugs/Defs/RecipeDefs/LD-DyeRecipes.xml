<RecipeDefs>

	<RecipeDef>
	    <defName>DyeClothRed</defName>
	    <label>Dye Cloth Red</label>
	    <description>Make a bundle of cloth red.</description>
	    <jobString>dyeing cloth.</jobString>
	    <workAmount>600</workAmount>
	    <workSpeedStat>SculptingSpeed</workSpeedStat>
	    <effectWorking>Tailor</effectWorking>
	    <soundWorking>Recipe_Tailor</soundWorking>
		
	    <ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Cloth</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
	    </ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Cloth</li>
			</thingDefs>
		</fixedIngredientFilter>
	    <products>
	      <RedCloth>10</RedCloth>
	    </products>
	    <workSkill>Crafting</workSkill>
	    <recipeUsers>
	      <li>BarrelDye</li>
	    </recipeUsers>
  	</RecipeDef>


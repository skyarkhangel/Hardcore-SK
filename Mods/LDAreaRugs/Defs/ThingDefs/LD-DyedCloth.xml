<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
	<ThingDef Name="ClothBase" Abstract="True">
    <thingClass>ThingWithComps</thingClass>
    <label>unspecified resource</label>
    <category>Item</category>
    <graphicData>
		<texPath>Things/Item/Resource/Cloth</texPath>
		<graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <resourceReadoutPriority>Middle</resourceReadoutPriority>
    <useHitPoints>true</useHitPoints>
    <selectable>true</selectable>
    <altitudeLayer>Item</altitudeLayer>
    <alwaysHaulable>true</alwaysHaulable>
    <drawGUIOverlay>true</drawGUIOverlay>
    <rotatable>false</rotatable>
    <pathCost>15</pathCost>
    <comps>
      <li>
        <compClass>CompForbiddable</compClass>
      </li>
    </comps>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<MarketValue>1.5</MarketValue>
			<Flammability>4.0</Flammability>
			<DeteriorationRate>0</DeteriorationRate>
		</statBases>
		<tickerType>Rare</tickerType>
		<stackLimit>300</stackLimit>
		<stuffProps>
			<categories>
				<li>Fabric</li>
			</categories>
			<statFactors>
				<MaxHitPoints>1.0</MaxHitPoints>
				<Beauty>1.1</Beauty>
				<ArmorRating_Blunt>1.3</ArmorRating_Blunt>
				<ArmorRating_Sharp>1.1</ArmorRating_Sharp>
				<ArmorRating_Heat>1.2</ArmorRating_Heat>
				<ArmorRating_Electric>1.2</ArmorRating_Electric>
				<Insulation_Cold>1.04</Insulation_Cold>
				<Insulation_Heat>0.95</Insulation_Heat>
				<MoveSpeed>1</MoveSpeed>
				<MarketValue>0.8</MarketValue>
				<WorkToMake>0.9</WorkToMake>
				<BedRestEffectiveness>1.1</BedRestEffectiveness>
				<ImmunityGainSpeedFactor>0.9</ImmunityGainSpeedFactor>
				<MedicalTreatmentQualityFactor>0.9</MedicalTreatmentQualityFactor>
			</statFactors>
		</stuffProps>
		<thingCategories>
			<li>BTextiles</li>
		</thingCategories>
	</ThingDef>
  

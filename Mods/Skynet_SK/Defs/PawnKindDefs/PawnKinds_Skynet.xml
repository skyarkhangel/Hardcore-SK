<Defs>
    <!-- Bodyguard -->
    <PawnKindDef>
        <defName>ResistanceBodyguard</defName>
        <label>Terminator prototype T-X</label>
        <race>SkynetTerminatorTX</race>
        <combatPower>1400</combatPower>
        <chemicalAddictionChance>0</chemicalAddictionChance>
        <baseRecruitDifficulty>0.99</baseRecruitDifficulty>
        <backstoryCategory>Raider</backstoryCategory>
        <defaultFactionType>HumanResistance</defaultFactionType>
        <backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
        <maxGenerationAge>30</maxGenerationAge>
        <itemQuality>Normal</itemQuality>
        <isFighter>true</isFighter>
        <aiAvoidCover>true</aiAvoidCover>
        <gearHealthRange>
            <min>0.7</min>
            <max>1.6</max>
        </gearHealthRange>
        <apparelMoney>
            <min>10000</min>
            <max>12000</max>
        </apparelMoney>
        <apparelRequired>
            <li>Apparello_GSkin</li>
        </apparelRequired>
        <apparelTags>
            <li>SyndicateMedium</li>
            <li>SyndicateUnique</li>
        </apparelTags>
        <weaponMoney>
            <min>3300</min>
            <max>4100</max>
        </weaponMoney>
        <weaponTags>
            <li>ADR2</li>
        </weaponTags>
        <modExtensions>
            <li Class="CombatExtended.LoadoutPropertiesExtension">
                <primaryMagazineCount>
                    <min>24</min>
                    <max>25</max>
                </primaryMagazineCount>
                <sidearms>
                    <li>
                        <generateChance>0.7</generateChance>
                        <sidearmMoney>
                            <min>250</min>
                            <max>350</max>
                        </sidearmMoney>
                        <weaponTags>
                            <li>GrenadeDestructive</li>
                        </weaponTags>
                        <magazineCount>
                            <min>1</min>
                            <max>3</max>
                        </magazineCount>
                    </li>
                </sidearms>
            </li>
        </modExtensions>
        <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
        <apparelAllowHeadwearChance>0.4</apparelAllowHeadwearChance>
    </PawnKindDef>

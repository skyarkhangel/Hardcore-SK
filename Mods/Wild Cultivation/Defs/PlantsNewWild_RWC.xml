<Defs>

	<ThingDef ParentName="RWC_PlantBase">
		<defName>PlantWildCorn</defName>
		<label>wild corn plant</label>
		<description>A large grain plant which produces ears of delicious yellow seeds. Takes a relatively long time to grow, but yields lots of food, and takes a long time to spoil. Needs fertile soil to grow well.</description>
		<statBases>
			<MaxHitPoints>150</MaxHitPoints>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/PlantCorn</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>12</pathCost>
		<ingestible>
			<nutrition>0.30</nutrition>
		</ingestible>
		<plant>
			<fertilitySensitivity>1.0</fertilitySensitivity>
			<dieIfLeafless>false</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<harvestWork>400</harvestWork>
			<harvestedThingDef>RawCorn</harvestedThingDef>
			<harvestYield>27</harvestYield>
			<topWindExposure>0.35</topWindExposure>
			<growDays>11.3</growDays>
			<immatureGraphicPath>Things/Plant/PlantCorn_Harvest</immatureGraphicPath>
			<visualSizeRange>
				<min>0.3</min>
				<max>1.4</max>
			</visualSizeRange>
			<wildCommonalityMaxFraction>0.4</wildCommonalityMaxFraction>
			<wildClusterSizeRange>
				<min>2</min>
				<max>5</max>
			</wildClusterSizeRange>
			<wildClusterRadius>4</wildClusterRadius>
			<wildBiomes>
				<AridShrubland>0.05</AridShrubland>
				<BorealForest>0.01</BorealForest>
				<Desert>0.02</Desert>
				<ExtremeDesert>0.0</ExtremeDesert>
				<TemperateForest>0.03</TemperateForest>
				<TropicalRainforest>0.01</TropicalRainforest>
				<Tundra>0.0</Tundra>
			</wildBiomes>
		</plant>
	</ThingDef>


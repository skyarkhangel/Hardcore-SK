<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  
  
  <ThingDef ParentName="ResourceBase">
    <defName>SmallFossil</defName>
    <label>Small fossil</label>
    <description>A small fossil embedded in rock. Valued by certain exotic goods traders and collectors. Such rock are traditionally used by the natives of this rimworld for beautiful and interesting flooring. Can be found when mining sedimentary rocks.</description>
    <graphicData>
      <texPath>Things/Item/Resource/SmallFossil</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <altitudeLayer>ItemImportant</altitudeLayer>
    <soundInteract>Stone_Drop</soundInteract>
    <soundDrop>Stone_Drop</soundDrop>
    <useHitPoints>true</useHitPoints>
    <statBases>
      <MarketValue>200</MarketValue>
      <DeteriorationRate>0</DeteriorationRate>
      <Flammability>0</Flammability>
      <Mass>4.0</Mass>
      <Beauty>5</Beauty>
    </statBases>
    <stackLimit>20</stackLimit>
    <smallVolume>false</smallVolume>
  </ThingDef>
  

</Defs>

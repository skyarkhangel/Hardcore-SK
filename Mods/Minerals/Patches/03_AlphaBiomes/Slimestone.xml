              <DropProbability>0.5</DropProbability>
              <CountPerDrop>5</CountPerDrop>
            </li>
          </randomlyDropResources>
          <!-- Things this mineral replaces when a map is initialized -->
          <ThingsToReplace>
            <li>AB_SlimeStone</li>
          </ThingsToReplace>
          <!-- Tags which determine how some options behave  -->
          <tags>
            <li>rock</li>
            <li>wall</li>
            <li>weathered</li>
          </tags>

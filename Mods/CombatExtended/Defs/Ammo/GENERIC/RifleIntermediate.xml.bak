		<statBases>
			<Mass>0.013</Mass>
			<Bulk>0.02</Bulk>
		</statBases>
		<tradeTags>
			<li>CE_AutoEnableTrade</li>
			<li>CE_AutoEnableCrafting</li>
		</tradeTags>
		<thingCategories>
			<li>AmmoRifleIntermediate</li>
		</thingCategories>
	</ThingDef>


<Patch>

	<!-- ========== PawnKind Patches ========== -->

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/PawnKindDef[defName="Mech_Lancer"]</xpath>
		<value>
			<aiAvoidCover>false</aiAvoidCover>
		</value>
	</Operation>


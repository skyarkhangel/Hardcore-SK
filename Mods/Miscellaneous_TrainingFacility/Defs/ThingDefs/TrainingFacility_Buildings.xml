<?xml version="1.0" encoding="utf-8" ?>
<ThingDefs>

	<ThingDef ParentName="BuildingBase">
		<defName>ShootingRangeTarget</defName>
		<thingClass>TrainingFacility.Building_ShootingRange</thingClass>
		<label>shooting target</label>
		<description>The shooting range is a place where you can train your hand-eye coordination.</description>
		<graphicData>
			<texPath>Things/Building/TrainingFacility/ShootingRangeTarget</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<damageData>
				<rect>(0.3,0.3,0.7,0.7)</rect>
			</damageData>
		</graphicData>
		<altitudeLayer>Waist</altitudeLayer>
		<rotatable>true</rotatable>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>7500</WorkToMake>
			<Flammability>1.0</Flammability>
			<Beauty>-5</Beauty>
			<EntertainmentStrengthFactor>1</EntertainmentStrengthFactor>
		</statBases>
		<building>
			<watchBuildingStandDistanceRange>
				<min>8</min>
				<max>10</max>
			</watchBuildingStandDistanceRange>
			<watchBuildingStandRectWidth>3</watchBuildingStandRectWidth>
		</building>
		<size>(1,1)</size>
	<minifiedDef>MinifiedFurniture</minifiedDef>
	<stuffCategories>
	<li>Leathery</li>
	</stuffCategories>
		<costStuffCount>120</costStuffCount>
		<costList>
			<Sand>40</Sand>
			<Components>5</Components>
		</costList>
		<pathCost>120</pathCost>
		<designationCategory>Joy</designationCategory>
		<placeWorkers>
			<li>TrainingFacility.PlaceWorker_ShootingRange</li>
		</placeWorkers>
		<researchPrerequisites><li>SecurityI</li></researchPrerequisites>
	</ThingDef>


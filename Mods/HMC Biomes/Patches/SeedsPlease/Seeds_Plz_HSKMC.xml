<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>SeedsPlease</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Normal</success>
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>Defs</xpath>
          <value>
			<SeedsPlease.SeedDef ParentName="SeedBase">
				<defName>SeedIronPine</defName>
				<label>Iron Pine seeds</label>
				<sources>
					<li>Plant_TreeIronPineHMC</li>
				</sources>
				<graphicData><texPath>Things/Item/Seeds/TreePine</texPath></graphicData>
				<statBases>
					<MarketValue>10</MarketValue>
				</statBases>
			</SeedsPlease.SeedDef>
			<SeedsPlease.SeedDef ParentName="SeedBase">
				<defName>SeedBuckwheat</defName>
				<label>Buckwheat seeds</label>
				<sources>
					<li>Plant_Buckwheat</li>
				</sources>
				<graphicData><texPath>Things/Item/Seeds/Buckwheat</texPath></graphicData>
				<statBases>
					<MarketValue>10</MarketValue>
				</statBases>
			</SeedsPlease.SeedDef>
			<RecipeDef ParentName="ExtractSeed">
				<defName>ExtractSeedBuckwheat</defName>
				<label>extract buckwheat seeds</label>
				<description>Extract seeds from buckwheats.</description>
				<ingredients>
					<li>
						<filter>
							<thingDefs>
								<li>RawBuckwheat</li>
							</thingDefs>
						</filter>
						<count>4</count>
					</li>
				</ingredients>
				<fixedIngredientFilter>
					<thingDefs>
						<li>RawBuckwheat</li>
					</thingDefs>
				</fixedIngredientFilter>
				<products>
					<SeedBuckwheat>3</SeedBuckwheat>
				</products>
			</RecipeDef>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>		
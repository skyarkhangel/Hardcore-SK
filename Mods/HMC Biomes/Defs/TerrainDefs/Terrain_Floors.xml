<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <TerrainDef ParentName="FloorBase">
    <defName>HMC_IronWoodPlankFloor</defName>
    <label>ironwood plank floor</label>
    <renderPrecedence>250</renderPrecedence>
    <description>Ironwood plank flooring for that warm, homey feeling. While it is quick to lay, but due to the complexity of processing this type of wood, it is quite difficult to make something more complex than ordinary parquet.</description>
    <texturePath>Terrain/Surfaces/WoodFloor</texturePath>
	<burnedDef>BurnedWoodPlankFloor</burnedDef>
    <color>(135,155,153)</color>
    <statBases>
      <WorkToBuild>960</WorkToBuild>
	  <Flammability>0.055</Flammability>
	  <Beauty>2</Beauty>
      <Cleanliness>0.1</Cleanliness>
	  <CleaningTimeFactor>0.75</CleaningTimeFactor>
	  <FilthMultiplier>0.75</FilthMultiplier>
    </statBases>
	<isPaintable>true</isPaintable>
    <costList>
      <IronWoodPlankHMC>3</IronWoodPlankHMC>
    </costList>
    <constructEffect>ConstructWood</constructEffect>
    <designationHotKey>Misc4</designationHotKey>
    <researchPrerequisites>
      <li>Wooden_floor_A3</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>5</constructionSkillPrerequisite>
  </TerrainDef>

  <TerrainDef MayRequire="Ludeon.RimWorld.Ideology" ParentName="FloorBase">
    <defName>HMC_IronWoodTotemicFloor</defName>
    <label>ironwood totemic floor</label>
    <renderPrecedence>251</renderPrecedence>
    <description>Ironwood totemic flooring for that warm, homey feeling. Due to the complexity of processing this type of wood, it is quite difficult to make something more complex than ordinary parquet.</description>
    <texturePath>Terrain/Surfaces/TotemicBoards</texturePath>
	<burnedDef>BurnedWoodPlankFloor</burnedDef>
    <color>(135,155,153)</color>
    <statBases>
      <WorkToBuild>1920</WorkToBuild>
	  <Flammability>0.055</Flammability>
	  <Beauty>4</Beauty>
      <Cleanliness>0.2</Cleanliness>
	  <CleaningTimeFactor>0.5</CleaningTimeFactor>
	  <FilthMultiplier>0.5</FilthMultiplier>
    </statBases>
	<isPaintable>true</isPaintable>
    <costList>
      <IronWoodPlankHMC>6</IronWoodPlankHMC>
    </costList>
    <constructEffect>ConstructWood</constructEffect>
    <designationHotKey>Misc4</designationHotKey>
    <researchPrerequisites>
      <li>Wooden_floor_B1</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>10</constructionSkillPrerequisite>
  </TerrainDef>

</Defs>

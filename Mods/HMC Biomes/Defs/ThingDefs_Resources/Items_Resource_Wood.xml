				<BedRestEffectiveness>0.95</BedRestEffectiveness>
				<ImmunityGainSpeedFactor>1.0</ImmunityGainSpeedFactor>
				<WorkTableWorkSpeedFactor>0.88</WorkTableWorkSpeedFactor>
				<DoorOpenSpeed>0.95</DoorOpenSpeed>
				<MeleePenetrationFactor>0.8</MeleePenetrationFactor>
				<Mass>1</Mass>
			</statFactors>
		</stuffProps>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<isTechHediff>true</isTechHediff>
		<thingCategories>
			<li>Wooden</li>
		</thingCategories>
		<techHediffsTags>

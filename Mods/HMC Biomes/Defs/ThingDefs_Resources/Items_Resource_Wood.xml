<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="SK_ResourceVerbBase">
		<defName>IronWoodLogHMC</defName>
		<label>iron wood log</label>
		<description>Wood from genetically modified iron pine trees. Useful for building structures, weapons, and even simple prosthetics. A raw stick of wood can be used as a weapon in a pinch. It can be processed into planks or kindling.</description>
		<graphicData>
			<texPath>Things/Resource/IronWoodLog</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<equippedAngleOffset>-20</equippedAngleOffset>
		<soundInteract>Wood_Drop</soundInteract>
		<soundDrop>Wood_Drop</soundDrop>
		<tickerType>Rare</tickerType>
		<statBases>
			<MarketValue>1.7</MarketValue>
			<MaxHitPoints>175</MaxHitPoints>
			<Flammability>0.7</Flammability>
			<DeteriorationRate>0</DeteriorationRate>
			<StuffPower_Armor_Blunt>0.8</StuffPower_Armor_Blunt>

			<StuffPower_Insulation_Heat>0.3</StuffPower_Insulation_Heat>
			<SharpDamageMultiplier>0.55</SharpDamageMultiplier>
			<BluntDamageMultiplier>0.9</BluntDamageMultiplier>
			<MaxBurningTempCelsius>400</MaxBurningTempCelsius>
			<BurnDurationHours>0.6</BurnDurationHours>
			<MeleeCounterParryBonus>0.9</MeleeCounterParryBonus>
			<Mass>0.35</Mass>
			<Bulk>0.35</Bulk>
		</statBases>
		<techLevel>Medieval</techLevel>
		<stackLimit>300</stackLimit>
		<stuffProps>
			<stuffAdjective>iron wood log</stuffAdjective>
			<commonality>0.15</commonality>
			<categories>
				<li>WoodLogs</li>
			</categories>
			<color>(135,155,153)</color>
			<constructEffect>ConstructWood</constructEffect>
			<appearance>Planks</appearance>
			<soundImpactStuff>BulletImpact_Wood</soundImpactStuff>

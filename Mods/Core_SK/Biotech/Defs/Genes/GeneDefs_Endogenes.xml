    <iconPath>UI/Icons/Genes/Gene_MinorToxicityResistance</iconPath>
    <displayOrderInCategory>0</displayOrderInCategory>
    <statOffsets>
      <ToxicResistance>-0.2</ToxicResistance>
    </statOffsets>

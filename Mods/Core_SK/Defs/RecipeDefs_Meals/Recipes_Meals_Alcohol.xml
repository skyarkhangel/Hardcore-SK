<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- Yeast -->
	<RecipeDef>
		<defName>Makeyeast</defName>
		<label>Extract yeast</label>
		<description>Extracts yeast from common fungi. Produces 15.</description>
		<jobString>Extracting yeast.</jobString>
		<workSpeedStat>DrugCookingSpeed</workSpeedStat>
		<workAmount>800</workAmount>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Drug</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<recipeUsers>
			<li>Brewery</li>
			<li>ElectricBrewery</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>PlantFoodRaw</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>RawShimmershroom</li>
				<li>RawGlowbulb</li>
				<li>Rawmushroom</li>
			</thingDefs>
			<specialFiltersToDisallow>
				<li>AllowRotten</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<products>
			<Yeast>15</Yeast>
		</products>
		<skillRequirements>
			<Cooking>6</Cooking>
		</skillRequirements>
		<workSkill>Cooking</workSkill>
		<researchPrerequisite>Brewing</researchPrerequisite>
	</RecipeDef>
	

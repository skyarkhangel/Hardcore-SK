	<RecipeDef ParentName="CookMealSpecialBase_SK">
		<defName>CookMeatSoup</defName>
		<label>cook meat soup</label>
		<description>Prepares a meat soup from raw meat and a few raw plants. Produces 3.</description>
		<jobString>Cooking meat soup.</jobString>
		<soundWorking>Chemlab_Medicine_Drink</soundWorking>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
						<li>MeatSubRaw</li>
					</categories>
				</filter>
				<count>0.65</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>BasicPlantFoodRaw</li>
						<li>FungusPlantRaw</li>
						<li>ExtraPlantFoodRaw</li>
					</categories>
				</filter>
				<count>0.5</count>
			</li>
		</ingredients>
		<products>
			<MeatSoup>3</MeatSoup>
		</products>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
				<li>MeatSubRaw</li>
				<li>BasicPlantFoodRaw</li>
				<li>FungusPlantRaw</li>
				<li>ExtraPlantFoodRaw</li>
			</categories>
			<specialFiltersToDisallow>
				<li>AllowPlantFood</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>MeatRaw</li>
				<li>MeatSubRaw</li>
				<li>BasicPlantFoodRaw</li>
				<li>FungusPlantRaw</li>
				<li>ExtraPlantFoodRaw</li>
			</categories>
			<disallowedThingDefs>
				<li>Meat_Megaspider</li>
				<li>Meat_Human</li>
			</disallowedThingDefs>
		</defaultIngredientFilter>
		<skillRequirements>
			<Cooking>4</Cooking>
		</skillRequirements>
		<recipeUsers>
			<li>FueledStove</li>
			<li>ElectricStove</li>
		</recipeUsers>
		<researchPrerequisite>Food_B4</researchPrerequisite>
	</RecipeDef>


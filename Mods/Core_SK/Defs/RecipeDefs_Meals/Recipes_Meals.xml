			<categories>
				<li>FruitFoodRaw</li>
			</categories>
			<exceptedCategories>
				<li>EggsFertilized</li>
			</exceptedCategories>
		</defaultIngredientFilter>
		<skillRequirements>
			<li>
				<skill>Cooking</skill>
				<minLevel>6</minLevel>
			</li>
		</skillRequirements>
		<workSkill>Cooking</workSkill>

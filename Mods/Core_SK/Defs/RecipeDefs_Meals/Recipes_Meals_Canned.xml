</RecipeDef>

<RecipeDef>
	<defName>PickleVegetable</defName>
	<label>pickle vegetable</label>
	<description>Preserve vegetables in a salty brine to prevent spoiling.</description>
	<jobString>Pickling food.</jobString>
	<workSpeedStat>CookSpeed</workSpeedStat>
	<workAmount>1000</workAmount>
		<effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>          
	<allowMixingIngredients>true</allowMixingIngredients>
    <!-- <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass> -->
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawPotatoes</li>
            <li>RawCorn</li>
            <li>Raweggplant</li>
            <li>Rawpumpkin</li>
            <li>Rawbean</li>
            <li>RawTomatoes</li>
            <li>RawCarrots</li>
          </thingDefs>
        </filter>
        <count>30</count>
      </li>
	<li>
	  <filter>
		<thingDefs>
			<li>ChunkSalt</li>
		</thingDefs>
	  </filter>
		<count>8</count>
	</li>
	</ingredients>
	<fixedIngredientFilter>
	  <thingDefs>
            <li>RawPotatoes</li>
            <li>RawCorn</li>
            <li>Raweggplant</li>
            <li>Rawpumpkin</li>
            <li>Rawbean</li>
            <li>RawTomatoes</li>
            <li>RawCarrots</li>
			<li>ChunkSalt</li>
	  </thingDefs>
      <disallowedSpecialFilters>
        <li>AllowPlantFood</li>
        <li>AllowNonSmeltableWeapons</li>
	   <li>AllowRotten</li>
      </disallowedSpecialFilters>
	</fixedIngredientFilter>
	<products>
	  <PickledVeg>15</PickledVeg>
	</products>
    <defaultIngredientFilter>
      <thingDefs>
            <li>RawPotatoes</li>
            <li>RawCorn</li>
            <li>Raweggplant</li>
            <li>Rawpumpkin</li>
            <li>Rawbean</li>
            <li>RawTomatoes</li>
            <li>RawCarrots</li>
    		<li>ChunkSalt</li>
       </thingDefs>
      <exceptedCategories>
        <li>EggsFertilized</li>
    </exceptedCategories>
      <exceptedThingDefs>
        <li>Human_Meat</li>
      </exceptedThingDefs>
    </defaultIngredientFilter>
	<workSkill>Cooking</workSkill>
	<recipeUsers>
         <li>Canningstove</li>
			<li>TableButcherNeolithic</li>
			<li>TableButcher</li>
     </recipeUsers>
</RecipeDef>

<RecipeDef>
	<defName>SaltMeat</defName>

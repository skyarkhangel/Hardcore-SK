	<RecipeDef>
		<defName>PickleVegetable</defName>
		<label>pickle vegetable</label>
		<description>Preserve vegetables in a salty brine to prevent spoiling.</description>
		<jobString>Pickling vegetables.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<workAmount>1000</workAmount>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>          
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RawPotatoes</li>
						<li>RawCorn</li>
						<li>Raweggplant</li>
						<li>Rawbean</li>
						<li>RawTomatoes</li>
						<li>RawCarrots</li>
						<li>RawLettuce</li>
						<li>Rawmushroom</li>
						<li>RawAlgae</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ChunkSalt</li>
					</thingDefs>
				</filter>
				<count>0.05</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>RawPotatoes</li>
				<li>RawCorn</li>
				<li>Raweggplant</li>
				<li>Rawbean</li>
				<li>RawTomatoes</li>
				<li>RawCarrots</li>
				<li>RawLettuce</li>
				<li>Rawmushroom</li>
				<li>RawAlgae</li>
				<li>ChunkSalt</li>
			</thingDefs>
			<disallowedSpecialFilters>
				<li>AllowPlantFood</li>
				<li>AllowNonSmeltableWeapons</li>
				<li>AllowRotten</li>
			</disallowedSpecialFilters>
		</fixedIngredientFilter>
		<products>
			<PickledVeg>15</PickledVeg>
		</products>
		<defaultIngredientFilter>
			<thingDefs>
				<li>RawPotatoes</li>
				<li>RawCorn</li>
				<li>Raweggplant</li>
				<li>Rawbean</li>
				<li>RawTomatoes</li>
				<li>RawCarrots</li>
				<li>RawLettuce</li>
				<li>Rawmushroom</li>
				<li>RawAlgae</li>
				<li>ChunkSalt</li>
			</thingDefs>
			<exceptedCategories>
				<li>EggsFertilized</li>
			</exceptedCategories>
			<exceptedThingDefs>
				<li>Human_Meat</li>
			</exceptedThingDefs>
		</defaultIngredientFilter>
		<workSkill>Cooking</workSkill>
		<recipeUsers>
			<li>Canningstove</li>
		</recipeUsers>
	</RecipeDef>

	<RecipeDef>
		<defName>SaltMeat</defName>
		<label>salt meat</label>
		<description>Preserve meats in salt to prevent spoiling.</description>
		<jobString>Salting meat.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<workAmount>1000</workAmount>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>          
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ChunkSalt</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<thingDefs>
				<li>ChunkSalt</li>
			</thingDefs>
			<disallowedSpecialFilters>
				<li>AllowPlantFood</li>
				<li>AllowNonSmeltableWeapons</li>
				<li>AllowRotten</li>
				<li>HumanlikeMeat</li>
				<li>InsectMeat</li>
			</disallowedSpecialFilters>
		</fixedIngredientFilter>
		<products>
			<SaltedMeat>10</SaltedMeat>
		</products>
		<defaultIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<thingDefs>
				<li>ChunkSalt</li>
			</thingDefs>
			<exceptedCategories>
				<li>EggsFertilized</li>
			</exceptedCategories>
			<exceptedThingDefs>
				<li>Human_Meat</li>
				<li>SaltedMeat</li>
			</exceptedThingDefs>
		</defaultIngredientFilter>
		<workSkill>Cooking</workSkill>
		<recipeUsers>
			<li>Canningstove</li>
			<li>TableButcher</li>
		</recipeUsers>
	</RecipeDef>

</Defs>
<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>MakeBasicCannedFood</defName>
		<label>cook basic canned food</label>
		<description>Preserve plant ingredients in cans to prevent spoiling. Produces 75.</description>
		<jobString>Canning plant ingredients.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<workAmount>1800</workAmount>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>          
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>BasicPlantFoodRaw</li>
					</categories>
				</filter>
				<count>75</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>MetalCan</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>BasicPlantFoodRaw</li>
			</categories>
			<thingDefs>
				<li>MetalCan</li>
			</thingDefs>
			<exceptedThingDefs>
				<li>BasicCannedFood</li>
				<li>ExtraCannedFood</li>
				<li>MetalCannedMeat_raw</li>
				<li>MetalCannedMeat_prime</li>
			</exceptedThingDefs>
		</fixedIngredientFilter>
		<products>
			<BasicCannedFood>75</BasicCannedFood>
		</products>
		<workSkill>Cooking</workSkill>
	</RecipeDef>



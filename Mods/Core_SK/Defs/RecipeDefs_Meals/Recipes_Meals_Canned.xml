        <li>MeatRaw</li>
      </categories>
	 <thingDefs>
		<li>ChunkSalt</li>
	 </thingDefs>
      <exceptedCategories>
        <li>EggsFertilized</li>
      </exceptedCategories>
      <exceptedThingDefs>
        <li>Human_Meat</li>
        <li>SaltedMeat</li>
      </exceptedThingDefs>
    </defaultIngredientFilter>
	<workSkill>Cooking</workSkill>
	<recipeUsers>
         <li>Canningstove</li>
			<li>TableButcherNeolithic</li>
			<li>TableButcher</li>
    </recipeUsers>
</RecipeDef>

</RecipeDefs>

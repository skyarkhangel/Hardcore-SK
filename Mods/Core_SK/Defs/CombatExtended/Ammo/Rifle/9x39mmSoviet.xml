  <RecipeDef ParentName="AmmoRecipeRifle">
    <defName>MakeAmmo_9x39mmSoviet_FMJ</defName>
    <label>make 9x39mm Soviet (FMJ) cartridge x60</label>
    <description>Craft 60 9x39mm Soviet (FMJ) cartridges.</description>
    <jobString>Making 9x39mm Soviet (FMJ) cartridges.</jobString>
    <ingredients>
      <li>
        <filter>
          <categories>
			<li>SLDBar</li>
			<li>USLDBar</li>
          </categories>
        </filter>
        <count>11</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>HeavyBar</li>
          </categories>
        </filter>
        <count>4</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Powder</li>
          </thingDefs>
        </filter>
        <count>3</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>SLDBar</li>
        <li>USLDBar</li>
		<li>HeavyBar</li>
      </categories>
      <thingDefs>
        <li>Powder</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_9x39mmSoviet_FMJ>60</Ammo_9x39mmSoviet_FMJ>
    </products>
	<skillRequirements>
		<Crafting>8</Crafting>
	</skillRequirements>
    <workSkill>Crafting</workSkill>
    <workSkillLearnFactor>0.8</workSkillLearnFactor>
  </RecipeDef>
  

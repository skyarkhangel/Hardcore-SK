	<RecipeDef>
		<defName>BuildHarpoon</defName>
		<label>Craft Harpoon</label>
		<description>Crafts a Harpoon. Produces 1.</description>
		<jobString>Crafting Harpoon.</jobString>
		<workAmount>1400</workAmount>
		<workSkill>Crafting</workSkill>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Smith</effectWorking>
		<soundWorking>Interact_Repair</soundWorking>
		<skillRequirements>
			<li>
				<skill>Crafting</skill>
				<minLevel>0</minLevel>
			</li>
		</skillRequirements>
    	<workSkillLearnFactor>0.8</workSkillLearnFactor>
		<ingredients>
			<li>
				<filter>
					<categories>
					<li>Woody</li>
					</categories>
				</filter>
				<count>70</count>
			</li>
			<li>
				<filter>
					<categories>
					<li>Metallic</li>
					</categories>
				</filter>
				<count>20</count>
			</li>
		</ingredients>
		<products>
			<Harpoon>1</Harpoon>
		</products>
		<fixedIngredientFilter>
			<categories>
				<li>HCMBar</li>
			<li>SLDBar</li>
			<li>USLDBar</li>
			<li>Woody</li>
			</categories>
		</fixedIngredientFilter>
		  <recipeUsers>
			<li>FueledSmithy</li>
			<li>ElectricSmithy</li>
			<li>FishingPier</li>
		  </recipeUsers>
		  <unfinishedThingDef>UnfinishedWeapon</unfinishedThingDef>
		  <researchPrerequisite>SK_FishingI</researchPrerequisite>
	</RecipeDef>
  

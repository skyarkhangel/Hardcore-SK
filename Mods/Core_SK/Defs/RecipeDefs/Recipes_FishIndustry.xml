<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

  <RecipeDef>
    <defName>MakeFishingRod</defName>
    <label>Craft Fishing Rod</label>
    <description>Make a Fishing Rod with some wood, a string and a piece of metal for the hook. Produces 1.</description>
    <jobString>Crafting Fishing Rod.</jobString>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>800</workAmount>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Woody</li>
          </categories>
        </filter>
        <count>20</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>BTextiles</li>
          </categories>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
					<categories>
						<li>Metallic</li>
                    </categories>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
						<li>BTextiles</li>
						<li>Woody</li>
                    </categories>
    </fixedIngredientFilter>
    <products>
      <FishingRod>1</FishingRod>
    </products>
    <workSkill>Crafting</workSkill>
	<unfinishedThingDef>UnfinishedWeapon</unfinishedThingDef>
    <researchPrerequisite>SK_FishingI</researchPrerequisite>
  </RecipeDef>

  
  <RecipeDef>
    <defName>ShellMashgon</defName>
    <label>Shell Mashgons</label>
    <jobString>Shelling Mashgons.</jobString>
    <description>Shell Mashgons to get small but tasty pieces of Mashgon meat. Produces 20.</description>
    <workAmount>700</workAmount>
    <workSkill>Cooking</workSkill>
    <workSpeedStat>ButcheryFleshSpeed</workSpeedStat>
    <efficiencyStat>ButcheryFleshEfficiency</efficiencyStat>
    <effectWorking>ButcherFlesh</effectWorking>
    <soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Fish_Mashgon</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Fish_Mashgon</li>
      </thingDefs>
      <specialFiltersToDisallow>
        <li>AllowRotten</li>
      </specialFiltersToDisallow>
    </fixedIngredientFilter>
    <products>
      <RawSeaFood_Mashgon>20</RawSeaFood_Mashgon>
    </products>
    <recipeUsers>
      <li>TableButcher</li>
    </recipeUsers>
    <researchPrerequisite>SK_FishingI</researchPrerequisite>
  </RecipeDef>

  

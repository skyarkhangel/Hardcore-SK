<Defs>

	<RecipeDef>
		<defName>MakeTallow</defName>
		<label>render tallow</label>
		<description>Render tallow from meat. Produces 15.</description>
		<jobString>Creating fuel.</jobString>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>true</allowMixingIngredients>
		<workAmount>1200</workAmount>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
				</filter>
				<count>30</count>
			</li>
		</ingredients>
		<products>
			<Tallow>15</Tallow>
		</products>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<disallowedThingDefs>
				<li>Tofu</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<disallowedThingDefs>
				<li>Meat_Megaspider</li>
				<li>Meat_Human</li>
			</disallowedThingDefs>
		</defaultIngredientFilter>

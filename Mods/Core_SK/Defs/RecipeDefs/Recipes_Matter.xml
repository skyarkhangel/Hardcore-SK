<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

  <!-- ============================================================== -->
  <!-- Convert to matter -->
  <!-- ============================================================== -->
  
    <!-- Raw resources -->
    <RecipeDef>
    <defName>ConvertResourcesRaw</defName>
    <label>Make Inorganic Matter</label>
    <description>Make Inorganic Matter by processing and converting various types of raw resources. Produces 15.</description>
    <jobString>Converting resources to matter.</jobString>
    <workAmount>1000</workAmount>
		<workSpeedStat>ElectronicCraftingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
    <soundWorking>Recipe_Smelt</soundWorking>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>ResourcesRaw</li>
          </categories>
        </filter>
        <count>30</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>ResourcesRaw</li>
      </categories>
    </fixedIngredientFilter>
    <products>
      <Matter>15</Matter>
    </products>
		<skillRequirements>
			<li>
				<skill>Crafting</skill>
				<minLevel>16</minLevel>
			</li>
		</skillRequirements>
	<workSkill>Crafting</workSkill>
    	<workSkillLearnFactor>0.8</workSkillLearnFactor>
  </RecipeDef>
  
 

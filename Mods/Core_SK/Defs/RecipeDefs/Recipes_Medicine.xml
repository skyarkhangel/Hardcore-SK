            </filter>
            <count>3</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>MintLeaves</li>
               </thingDefs>
            </filter>
            <count>3</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Paraffins</li>
               </thingDefs>

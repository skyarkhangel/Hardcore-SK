<?xml version="1.0" encoding="utf-8" ?>
<RecipeDefs>

<RecipeDef>
<defName>MakeHerbMedicine</defName>
<label>Craft Herbal Medicine</label>
<description>Crafts herbal medicine from Raw Healroot. Produces 1.</description>
<jobString>Crafting herbal medicine.</jobString>
<workAmount>500</workAmount>
	<workSpeedStat>CookSpeed</workSpeedStat>
	<effectWorking>Cook</effectWorking>
	<soundWorking>Chemlab_Herbal</soundWorking>
<ingredients>
	<li>
<filter>
<thingDefs>
<li>HerbalMedicine</li>
</thingDefs>
</filter>
<count>8</count>
</li>
<li>
<filter>
<thingDefs>
<li>Ash</li>
</thingDefs>
</filter>
<count>2</count>
</li>
</ingredients>
<products>
<HerbMedicine>1</HerbMedicine>
</products>
<fixedIngredientFilter>
<thingDefs>
<li>HerbalMedicine</li>
<li>Ash</li>
</thingDefs>
</fixedIngredientFilter>
		<skillRequirements>
			<li>
				<skill>Medicine</skill>
				<minLevel>6</minLevel>
			</li>
		</skillRequirements>
<workSkill>Medicine</workSkill>
<workSkillLearnFactor>0.8</workSkillLearnFactor>
</RecipeDef>


<RecipeDef>
	<defName>Makeherbmeds</defName>
	<label>Make Herbal Medical Kit</label>
	<description>Combines herbs into a medical kit. Produces 1.</description>
	<jobString>Mixing herbs into medicine kit.</jobString>
	<workAmount>800</workAmount>
	<workSpeedStat>CookSpeed</workSpeedStat>
	<effectWorking>Cook</effectWorking>
	<soundWorking>Chemlab_Herbal</soundWorking>
	<allowMixingIngredients>true</allowMixingIngredients>
	<ingredients>
	<li>
	    <filter>
	      <thingDefs>
	        <li>aloe</li>
	      </thingDefs>
	    </filter>
	    <count>8</count>
	  </li>
	<li>
	    <filter>
	      <thingDefs>
	        <li>WeedOil</li>
	      </thingDefs>
	    </filter>
	    <count>1</count>
	  </li>
     <li>
	    <filter>
	      <thingDefs>
	        <li>HerbalMedicine</li>
	      </thingDefs>
	    </filter>
	    <count>1</count>
	  </li>
	</ingredients>
	<fixedIngredientFilter>
	    <thingDefs>
	        <li>aloe</li>
			<li>WeedOil</li>
	        <li>HerbalMedicine</li>			
	    </thingDefs>
	</fixedIngredientFilter>
	<products>
	  <HerbalMedicaKit>1</HerbalMedicaKit>
	</products>
		<skillRequirements>
			<li>
				<skill>Medicine</skill>
				<minLevel>12</minLevel>
			</li>
		</skillRequirements>
	<workSkill>Medicine</workSkill>
    <workSkillLearnFactor>0.8</workSkillLearnFactor>
</RecipeDef>


<RecipeDef>
   <defName>makemedicine</defName>
   <label>Craft Medicine</label>
   <description>Crafts a medicine kit from herbal medicines. Produces 1.</description>
   <jobString>Crafting medicine.</jobString>
   <workAmount>800</workAmount>
	<workSpeedStat>SmeltingSpeed</workSpeedStat>
	<effectWorking>Cook</effectWorking>
	<soundWorking>Chemlab_Medicine</soundWorking>
   <ingredients>
      <li>
         <filter>
            <thingDefs>
               <li>HerbalMedicine</li>
            </thingDefs>
         </filter>
         <count>6</count>
      </li>
      <li>
         <filter>
            <thingDefs>
               <li>Paraffins</li>
            </thingDefs>
         </filter>
         <count>2</count>
      </li>
      <li>
         <filter>
            <thingDefs>
               <li>SyntheticFibers</li>
            </thingDefs>
         </filter>
         <count>2</count>
      </li>
   </ingredients>
   <products>
      <Medicine>1</Medicine>
   </products>
   <fixedIngredientFilter>
      <thingDefs>
         <li>HerbalMedicine</li>
         <li>SyntheticFibers</li>
         <li>Paraffins</li>
      </thingDefs>
   </fixedIngredientFilter>
 		<skillRequirements>
			<li>
				<skill>Medicine</skill>
				<minLevel>10</minLevel>
			</li>
		</skillRequirements>
   <workSkill>Medicine</workSkill>
   <workSkillLearnFactor>0.8</workSkillLearnFactor>
</RecipeDef>


   <RecipeDef>
      <defName>makelifesupport</defName>
      <label>Make Life Support Device</label>
      <description>Makes a life support device. Produces 1.</description>
      <jobString>Making life support device.</jobString>
      <workAmount>2500</workAmount>
	<workSpeedStat>CookSpeed</workSpeedStat>
	<effectWorking>Cook</effectWorking>
	<soundWorking>Chemlab_Medicine</soundWorking>
      <ingredients>
         <li>
            <filter>
				<categories>
				<li>Metallic</li>
                </categories>
            </filter>
            <count>10</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Component</li>
               </thingDefs>
            </filter>
            <count>10</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Medicine</li>
               </thingDefs>
            </filter>
            <count>3</count>
         </li>
      </ingredients>
      <products>
         <lifesupport>1</lifesupport>
      </products>
      <fixedIngredientFilter>
		<categories>
		<li>SLDBar</li>
		<li>USLDBar</li>
         </categories>
         <thingDefs>
            <li>Component</li>
            <li>Medicine</li>
         </thingDefs>
      </fixedIngredientFilter>
		<skillRequirements>
			<li>
				<skill>Medicine</skill>
				<minLevel>6</minLevel>
			</li>
		</skillRequirements>
      <workSkill>Medicine</workSkill>
    <workSkillLearnFactor>0.8</workSkillLearnFactor>
   </RecipeDef>


   <RecipeDef>

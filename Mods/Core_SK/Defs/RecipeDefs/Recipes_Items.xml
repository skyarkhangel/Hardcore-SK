<Defs>

  <RecipeDef>
    <defName>MakeMiningHelmet</defName>
    <label>Craft mining helmet</label>
    <description>Crafts a mining helmet. Has a built in light and increases mining speed. Produces 1.</description>
    <jobString>Crafting a mining helmet.</jobString>
    <workAmount>9000</workAmount>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Interact_Repair</soundWorking>
	<recipeUsers>
      <li>FueledSmithy</li>
      <li>ElectricSmithy</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
			<categories>
				<li>Metallic</li>
            </categories>
        </filter>
        <count>80</count>
      </li>
      <li>
        <filter>
			<categories>
				<li>Textiles</li>
            </categories>
        </filter>
        <count>40</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
						<li>BTextiles</li>
						<li>HTextiles</li>
                    </categories>
    </fixedIngredientFilter>
    <products>
      <MiningHelmet>1</MiningHelmet>    
	</products>
		<skillRequirements>
			<Crafting>8</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
    	<workSkillLearnFactor>0.8</workSkillLearnFactor>
		<unfinishedThingDef>UnfinishedMetallicApparel</unfinishedThingDef>
  </RecipeDef>
  
  

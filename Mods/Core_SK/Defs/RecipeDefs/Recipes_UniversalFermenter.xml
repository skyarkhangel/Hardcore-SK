	<!--___________________JerkyRack_____________________-->
	<RecipeDef Name="Make_JerkyBase" Abstract="True">
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<disallowedThingDefs>
				<li>Meat_Human</li>
				<li>SaltedMeat</li>
				<li>Tofu</li>
				<li>soylentgreen</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<recipeUsers>
			<li>JerkyRack</li>
		</recipeUsers>
		
		<!-- RecipeDef_UF fields -->
		<temperatureSafe>
			<min>10</min>
			<max>55</max>
		</temperatureSafe>
		<temperatureIdeal>
			<min>25</min>
			<max>40</max>
		</temperatureIdeal>
		<processDays>2</processDays>
		<speedBelowSafe>0.6</speedBelowSafe>
		<speedAboveSafe>1</speedAboveSafe>
		<sunFactor>
			<min>0.75</min>
			<max>1.2</max>
		</sunFactor>
		<rainFactor>
			<min>1</min>
			<max>0</max>
		</rainFactor>
		<snowFactor>
			<min>1</min>
			<max>0</max>
		</snowFactor>
		<windFactor>
			<min>1</min>
			<max>1.2</max>
		</windFactor>
	</RecipeDef>
	

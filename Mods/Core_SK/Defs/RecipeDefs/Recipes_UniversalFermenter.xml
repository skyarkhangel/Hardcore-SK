	<!--___________________JerkyRack_____________________-->
	<RecipeDef>
		<defName>Make_Jekry</defName>
		<label>make jerky</label>
		<description>Make jerky meat. Takes 2 days to make</description>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MeatRaw</li>
					</categories>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>MeatRaw</li>
			</categories>
			<disallowedThingDefs>
				<li>Meat_Human</li>
				<li>SaltedMeat</li>
				<li>Tofu</li>
				<li>soylentgreen</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<products>
			<MealJerky>10</MealJerky>
		</products>
	</RecipeDef>


<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>MakeElectronicComponents</defName>
		<label>Assemble electronic components</label>
		<description>Assemble electronic components. Vital part of all electronics. Produces 5.</description>
		<jobString>Assembling electronic components.</jobString>
		<workAmount>2500</workAmount>
		<workSpeedStat>ElectronicCraftingSpeed</workSpeedStat>
		<effectWorking>Smith</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<recipeUsers>
			<li>TableElectronics</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>HCMBar</li>
					</categories>
				</filter>
				<count>35</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Glass</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SilverBar</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Glass</li>
				<li>ComponentIndustrial</li>
				<li>SilverBar</li>
			</thingDefs>
			<categories>
				<li>HCMBar</li>
			</categories>
		</fixedIngredientFilter>
		<products>
			<ElectronicComponents>5</ElectronicComponents>
		</products>
		<skillRequirements>
			<Crafting>8</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.8</workSkillLearnFactor>
	</RecipeDef>



              <li>
                <capacity>Moving</capacity>
                <offset>0.1</offset>
              </li>
              <li>
                <capacity>Consciousness</capacity>
                <offset>0.5</offset>
              </li>
            </capMods>
        </li>
        <li>
          <minSeverity>0.5</minSeverity>
          <label>jacked up</label>
          <painFactor>0.8</painFactor>
            <capMods>
              <li>
                <capacity>Moving</capacity>
                <offset>0.2</offset>
              </li>
              <li>
                <capacity>Consciousness</capacity>
                <offset>0.15</offset>
              </li>
            </capMods>
        </li>
      </stages>
  </HediffDef>




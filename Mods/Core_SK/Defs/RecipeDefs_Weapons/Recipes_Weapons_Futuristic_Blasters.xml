<Defs>

	<RecipeDef>
		<defName>BuildPhalanx</defName>
		<label>Build A-1 Phalanx</label>
		<description>A close-in weapon to be used with no loss of stopping power in comparison with a soldier's assault rifle. The Phalanx enjoys a ballistics advantage over most heavy pistols. Civilian variants are often purchased by colonists on planets that have particularly dangerous big-game animals.</description>
		<jobString>Building A-1 Phalanx</jobString>
		<workAmount>19000</workAmount>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Smith</effectWorking>
		<soundWorking>Interact_Repair</soundWorking>
		<skillRequirements>
			<Crafting>20</Crafting>
		</skillRequirements>
		<workSkillLearnFactor>0.8</workSkillLearnFactor>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Plasma_Component</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Weapon_Parts</li>
					</thingDefs>
				</filter>
				<count>12</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Component</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>Metallic</li>
					</categories>
				</filter>
				<count>50</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Plastic</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
		</ingredients>
		<products>
			<Gun_Phalanx>1</Gun_Phalanx>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Plasma_Component</li>
				<li>Weapon_Parts</li>
				<li>Component</li>
				<li>Plastic</li>
			</thingDefs>
			<categories>
				<li>SLDBar</li>
				<li>USLDBar</li>
			</categories>
		</fixedIngredientFilter>
		<workSkill>Crafting</workSkill>
		<recipeUsers>
			<li>MechWeaponCraftingWorkTable</li>
		</recipeUsers>

	<ThingDef ParentName="SK_BuildingBase">
		<defName>OpenBlastDoor</defName>
		<label>large armor gate (open)</label>
		<description>A very large gate that provides protection against hostile outside entities. It requires power, and must be manually opened and closed. The gate is currently open.</description>
		<thingClass>SK.Building_OpenedBlastDoor</thingClass>
		<graphicData>
			<texPath>Things/Building/ArmorGate/ArmorGateOpen</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(3,1)</drawSize>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<altitudeLayer>DoorMoveable</altitudeLayer>
		<neverMultiSelect>true</neverMultiSelect>
		<rotatable>true</rotatable>
		<size>(3,1)</size>
		<terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>	
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<holdsRoof>true</holdsRoof>
		<passability>Standable</passability>
		<fillPercent>0.3</fillPercent>
		<statBases>
			<WorkToBuild>10</WorkToBuild>
			<MaxHitPoints>2000</MaxHitPoints>
			<Flammability>0</Flammability>
		</statBases>
		<tickerType>Normal</tickerType>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>350</basePowerConsumption>
			</li>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<blockLight>false</blockLight>
		<researchPrerequisites>
			<li>SK_ConstructionIII</li>
		</researchPrerequisites>
		<constructionSkillPrerequisite>14</constructionSkillPrerequisite>
	</ThingDef>



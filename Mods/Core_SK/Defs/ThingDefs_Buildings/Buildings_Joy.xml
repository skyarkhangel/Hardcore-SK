			<li>PlaceWorker_WatchArea</li>
		</placeWorkers>
		<researchPrerequisites>
			<li>Enjoyment_A2</li>
		</researchPrerequisites>
		<designationHotKey>Misc1</designationHotKey>
		<constructionSkillPrerequisite>2</constructionSkillPrerequisite>

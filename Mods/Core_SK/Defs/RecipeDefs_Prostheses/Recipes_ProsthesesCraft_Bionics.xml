   <RecipeDef ParentName="RecipeBionicProsthesis">
      <defName>CreateBionicEye</defName>
      <label>Craft Bionic Eye</label>
      <description>Crafts a bionic eye. Produces 1.</description>
      <jobString>Crafting bionic eye.</jobString>
      <ingredients>
         <li>
            <filter>
               <thingDefs>
                  <li>Biomatter</li>
               </thingDefs>
            </filter>
            <count>30</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>ReinforcedGlass</li>
               </thingDefs>
            </filter>
            <count>10</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Paraffins</li>
               </thingDefs>
            </filter>
            <count>10</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Plastic</li>
               </thingDefs>
            </filter>
            <count>15</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Microchips</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <thingDefs>
            <li>Biomatter</li>
            <li>ReinforcedGlass</li>
            <li>Paraffins</li>
            <li>Plastic</li>
            <li>Microchips</li>
         </thingDefs>
      </fixedIngredientFilter>
      <products>
         <BionicEye>1</BionicEye>
      </products>
		<skillRequirements>
			<Crafting>14</Crafting>
		</skillRequirements>
   </RecipeDef>
 

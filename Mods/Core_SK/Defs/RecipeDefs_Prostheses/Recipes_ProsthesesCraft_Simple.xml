         <li>
            <filter>
               <thingDefs>
                  <li>Mechanism</li>
               </thingDefs>
            </filter>
            <count>5</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
                    </categories>

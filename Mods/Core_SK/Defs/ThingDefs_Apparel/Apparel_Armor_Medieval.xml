			<wornGraphicPath>Things/Apparel/Apparellonew/Trypophobia/LC</wornGraphicPath>
			<layers>
				<li>Shell</li>
			</layers>
			<tags>
				<li>Outlander</li>
				<li>RenegadesLight</li>
				<li>RenegadesMedium</li>
				<li>PredatorsLight</li>
				<li>EmpireLight</li>
				<li>EmpireScout</li>

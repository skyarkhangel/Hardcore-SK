<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="ApparelIndustrialBase">
		<defName>Apparel_FlakJacket</defName>
		<label>Suit jacket</label>
		<description>A fancy suit jacket that can be worn on top of a shirt. You can look your best at any time of the day in a suit!</description>
		<graphicData>
			<texPath>Things/Apparel/Marines/Apparel/MARSOC</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>		
		<stuffCategories>
			<li>HF</li>
		</stuffCategories>
		<costStuffCount>95</costStuffCount>
		<costList>
			<ComponentIndustrial>7</ComponentIndustrial>
			<SyntheticFibers>10</SyntheticFibers>
		</costList>
		<statBases>
			<WorkToMake>15000</WorkToMake>
			<MaxHitPoints>115</MaxHitPoints>
			<Flammability>1</Flammability>
			<Mass>1</Mass>
			<Bulk>15</Bulk>
			<WornBulk>7</WornBulk>
			<EquipDelay>3.8</EquipDelay>
			<Insulation_Heat>5</Insulation_Heat>
			<Insulation_Cold>5</Insulation_Cold>			
			<StuffEffectMultiplierInsulation_Cold>0.75</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.75</StuffEffectMultiplierInsulation_Heat>
			<StuffEffectMultiplierArmor>1.6</StuffEffectMultiplierArmor>
			<ArmorRating_Sharp>3.2</ArmorRating_Sharp>
			<ArmorRating_Blunt>4.56</ArmorRating_Blunt>
			<ArmorRating_Heat>0</ArmorRating_Heat>
		</statBases>
		<equippedStatOffsets>
			<CarryBulk>10</CarryBulk>
			<MoveSpeed>-0.2</MoveSpeed>
			<WorkSpeedGlobal>-0.1</WorkSpeedGlobal>
			<MentalBreakThreshold>-0.04</MentalBreakThreshold>
			<ShootingAccuracyPawn>-0.07</ShootingAccuracyPawn>
			<AimingDelayFactor>0.06</AimingDelayFactor>
			<MeleeHitChance>-0.06</MeleeHitChance>
			<MeleeWeapon_CooldownMultiplier>0.11</MeleeWeapon_CooldownMultiplier>
			<Suppressability>-0.15</Suppressability>
			<GermResistance>0.04</GermResistance>
			<GermContainment>0.03</GermContainment>
		</equippedStatOffsets>
		<generateCommonality>2</generateCommonality>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/Marines/Apparel/MARSOC</wornGraphicPath>
			<layers>
				<li>Shell</li>
			</layers>
			<tags>
				<li>BrotherhoodLight</li>
				<li>SectarianLight</li>
				<li>SectarianMedium</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(0.33,0.33,0.33,1)</only>
				</li>
				<li>
					<weight>10</weight>
					<only>(0.65,0.65,0.65,1)</only>
				</li>
				<li>
					<weight>30</weight>
					<only>(0.4,0.3,0.15,1)</only>
				</li>
				<li>
					<weight>6</weight>
					<min>(0.3,0.3,0.3,1)</min>
					<max>(0.5,0.5,0.5,1)</max>
				</li>
				<li>
					<weight>2</weight>
					<min>(0.5,0.5,0.5,1)</min>
					<max>(1,1,1,1)</max>
				</li>
			</options>
		</colorGenerator>
		<thingCategories>
			<li>MediumUpperArmorCat</li>
			<li>Setmarinearmor</li>
		</thingCategories>
        <recipeMaker>
            <researchPrerequisite>Armor_C1</researchPrerequisite>
        </recipeMaker>
	</ThingDef>


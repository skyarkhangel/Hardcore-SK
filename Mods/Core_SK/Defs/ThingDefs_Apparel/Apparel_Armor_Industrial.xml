<?xml version="1.0" encoding="utf-8" ?>

<Defs>

	<ThingDef ParentName="ApparelIndustrialBase">
		<defName>Apparel_NSuit</defName>
		<label>Marine Suit</label>
		<description>A fancy suit jacket that can be worn on top of a shirt. You can look your best at any time of the day in a suit!</description>
		<graphicData>
		<texPath>Things/Apparel/Marines/Apparel/MARSOC</texPath>
		<graphicClass>Graphic_Single</graphicClass>
		</graphicData>		
		<stuffCategories>
			<li>HF</li>
		</stuffCategories>
		<costStuffCount>95</costStuffCount>
		<costList>
			<Component>7</Component>
			<SyntheticFibers>10</SyntheticFibers>
		</costList>
		<statBases>
			<WorkToMake>15000</WorkToMake>
			<MaxHitPoints>115</MaxHitPoints>
			<Flammability>5</Flammability>
			<Insulation_Heat>5</Insulation_Heat>
			<Insulation_Cold>-24</Insulation_Cold>
			<ArmorRating_Blunt>0.5</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.55</ArmorRating_Sharp>
			<ArmorRating_Heat>0.2</ArmorRating_Heat>
			<ArmorRating_Electric>0.2</ArmorRating_Electric>
			<Mass>1</Mass>
			<Bulk>15</Bulk>
			<WornBulk>7</WornBulk>
			</statBases>
		<equippedStatOffsets>
			<CarryBulk>10</CarryBulk>
			<MoveSpeed>-0.55</MoveSpeed>
			<WorkSpeedGlobal>-0.2</WorkSpeedGlobal>
			<MentalBreakThreshold>-0.04</MentalBreakThreshold>
			<ShootingAccuracy>-0.07</ShootingAccuracy>
			<AimingDelayFactor>0.06</AimingDelayFactor>
			<MeleeHitChance>-0.06</MeleeHitChance>
			<MeleeWeapon_CooldownMultiplier>0.11</MeleeWeapon_CooldownMultiplier>
			<Suppressability>-0.15</Suppressability>
		</equippedStatOffsets>
					<generateCommonality>3</generateCommonality>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/Marines/Apparel/MARSOC</wornGraphicPath>
			<layers>
				<li>Shell</li>
			</layers>
		<tags>
			<li>BrotherhoodLight</li>
			<li>SectarianLight</li>
			<li>SectarianMedium</li>
		</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>RGBA(0.33,0.33,0.33,1)</only>
				</li>
				<li>
					<weight>10</weight>
					<only>RGBA(0.65,0.65,0.65,1)</only>
				</li>
				<li>
					<weight>30</weight>
					<only>RGBA(0.4,0.3,0.15,1)</only>
				</li>
				<li>
					<weight>6</weight>
					<min>RGBA(0.3,0.3,0.3,1)</min>
					<max>RGBA(0.5,0.5,0.5,1)</max>
				</li>
				<li>
					<weight>2</weight>
					<min>RGBA(0.5,0.5,0.5,1)</min>
					<max>RGBA(1,1,1,1)</max>
				</li>
			</options>
		</colorGenerator>
		<thingCategories>
			<li>MediumUpperArmorCat</li>
			<li>Setmarinearmor</li>
		</thingCategories>
	</ThingDef>
	
	<ThingDef ParentName="ApparelIndustrialBase">

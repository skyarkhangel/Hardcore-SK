	<ThingDef ParentName="ApparelNeoliticBase">
    <defName>Apparel_TribalBackpack</defName>
    <label>tribal backpack</label>
    <description>Primitive backpack made from animal furs, useful for storing items when travelling.</description>
		<thingClass>CombatExtended.Apparel_VisibleAccessory</thingClass>
		<graphicData>
			<texPath>Things/Apparel/TribalPack/Tribalpack</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<tradeability>Sellable</tradeability>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>3000</WorkToMake>
			<Mass>1</Mass>
			<Bulk>3</Bulk>
		</statBases>
		<equippedStatOffsets>
			<CarryBulk>15</CarryBulk>
			<MoveSpeed>-0.17</MoveSpeed>
			<WorkSpeedGlobal>-0.14</WorkSpeedGlobal>
		</equippedStatOffsets>
		<stuffCategories>
			<li>Fabric</li>
			<li>Leathery</li>
		</stuffCategories>
		<costStuffCount>25</costStuffCount>
		<generateCommonality>1.5</generateCommonality>
		<apparel>
			<careIfWornByCorpse>false</careIfWornByCorpse>
			<bodyPartGroups>
				<li>Torso</li>
			</bodyPartGroups>
			<layers>
				<li>Belt</li>
			</layers>
			  <tags>
				<li>Tribal</li>
				<li>Outlander</li>
				<li>PredatorsLight</li>
				<li>PredatorsMedium</li>
				<li>PredatorsScout</li>
			  </tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
		  <options>
			<li>
			  <weight>10</weight>
			  <only>RGBA(0.4,0.3,0.15,1)</only>
			</li>
			<li>
			  <weight>15</weight>
			  <only>RGBA(0.6,0.45,0.18,1)</only>
			</li>
			<li>
			  <weight>20</weight>
			  <only>RGBA(0.8,0.6,0.23,1)</only>
			</li>
		  </options>
		</colorGenerator>
		<thingCategories>
			<li>SpecialApparelCat</li>
		</thingCategories>
        <modExtensions>
          <li Class="CombatExtended.VisibleAccessoryDefExtension">
            <order>2</order>
          </li>
        </modExtensions>
	</ThingDef>
	
</Defs>

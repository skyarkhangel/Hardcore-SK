<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="ArmorSpacerBase">
		<defName>Apparel_Jetpack</defName>
		<label>Jetpack</label>
		<description>The future is finally here with your own personalized jetpack, allowing you move around the map much faster than any competing colonist.</description>
		<graphicData>
			<texPath>Things/Apparel/Jetpack/Jetpack</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>	
		<statBases>
			<WorkToMake>12500</WorkToMake>
			<MaxHitPoints>80</MaxHitPoints>
			<Flammability>1</Flammability>
			<Mass>12</Mass>
			<Bulk>8</Bulk>
			<WornBulk>12</WornBulk>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>0.50</MoveSpeed>
			<WorkSpeedGlobal>0.20</WorkSpeedGlobal>
			<AimingDelayFactor>0.10</AimingDelayFactor>
			<ShootingAccuracy>-0.1</ShootingAccuracy>
		</equippedStatOffsets>
		<stuffCategories>
			<li>HF</li>
			<li>Metallic</li>
		</stuffCategories>
		<costStuffCount>25</costStuffCount>
		<costList>
			<Component>10</Component>
			<BiosyntheticMaterial>5</BiosyntheticMaterial>
			<MagneticMaterial>10</MagneticMaterial>
			<Carbon>5</Carbon>
			<Microchips>1</Microchips>
		</costList>
			<generateCommonality>1</generateCommonality>
		<apparel>
			<careIfWornByCorpse>false</careIfWornByCorpse>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>  
			</bodyPartGroups>
			<wornGraphicPath>Things/Apparel/Jetpack/Jetpack</wornGraphicPath>
			<layers>
				<li>Shell</li>
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<thingCategories>
			<li>SpecialApparelCat</li>
			<li>SetJet</li>
		</thingCategories>
	</ThingDef>

	<ThingDef ParentName="ApparelIndustrialBase">
		<defName>Apparel_TacVest</defName>
		<label>Loadbearing Kit</label>
		<thingClass>CombatExtended.Apparel_VisibleAccessory</thingClass>
		<description>A loadbearing vest with a number of pouches to store magazines and other equipment.\n\nSpeeds up reloading but is quite bulky to wear.</description>
		<graphicData>
			<texPath>Things/Apparel/TacRigging/TacRigging</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<WorkToMake>8000</WorkToMake>
			<Mass>2</Mass>
			<Bulk>10</Bulk>
			<WornBulk>8</WornBulk>
		</statBases>
		<equippedStatOffsets>
			<CarryBulk>30</CarryBulk>
			<ReloadSpeed>0.1</ReloadSpeed>
			<MoveSpeed>-0.06</MoveSpeed>
			<WorkSpeedGlobal>-0.05</WorkSpeedGlobal>
			<AimingDelayFactor>-0.1</AimingDelayFactor>
		</equippedStatOffsets>
		<stuffCategories>
			<li>HF</li>
			<li>Fabric</li>
		</stuffCategories>
		<costStuffCount>55</costStuffCount>
		<costList>
			<Component>3</Component>
		</costList>
			<generateCommonality>1</generateCommonality>
		<apparel>
			<careIfWornByCorpse>false</careIfWornByCorpse>
			<bodyPartGroups>
				<li>Shoulders</li>
			</bodyPartGroups>
			<layers>
				<li>Belt</li>
			</layers>
			<tags>
				<li>Outlander</li>
				<li>Military</li>
				<li>OrionLight</li>
				<li>OrionMedium</li>
				<li>OrionScout</li>
				<li>BrotherhoodLight</li>
				<li>BrotherhoodMedium</li>
				<li>BrotherhoodScout</li>
				<li>SectarianScout</li>
				<li>SectarianLight</li>
				<li>SectarianMedium</li>
				<li>SyndicateLight</li>
				<li>SyndicateMedium</li>
				<li>SyndicateScout</li>
				<li>SyndicateMedium</li>
				<li>AsariCommando</li>
				<li>AsariPowerArmor</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>RGBA(0.33,0.33,0.33,1)</only>
				</li>
			</options>
		</colorGenerator>
		<thingCategories>
			<li>SpecialApparelCat</li>
		</thingCategories>
        <modExtensions>
          <li Class="CombatExtended.VisibleAccessoryDefExtension">
            <order>1</order>
          </li>
        </modExtensions>
	</ThingDef>


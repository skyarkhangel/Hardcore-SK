			<growDays>12</growDays>
			<fertilitySensitivity>0.25</fertilitySensitivity>
			<harvestWork>550</harvestWork>
			<harvestYield>22</harvestYield>
			<harvestedThingDef>RawCoconut</harvestedThingDef>
			<fertilityMin>0.03</fertilityMin>
			<wildClusterRadius>10</wildClusterRadius>
			<wildClusterWeight>11</wildClusterWeight>
		</plant>

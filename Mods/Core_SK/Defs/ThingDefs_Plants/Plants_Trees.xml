	<ThingDef ParentName="SK_DeciduousTreeBase">
		<defName>Plant_TreeOak</defName>
		<label>oak tree</label>
		<description>Oak trees are slow growing, strong, hardwood trees.</description>
		<graphicData>
			<texPath>Things/Trees/TreeOak</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.17, 0.35, 0.13)</volume>
				<offset>(-0.03,0,-0.44)</offset>
			</shadowData>
		</graphicData>

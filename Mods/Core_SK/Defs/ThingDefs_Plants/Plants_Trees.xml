<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--============================ Trees  ============================-->

	<ThingDef ParentName="TreewoodBase">
		<thingClass>SK.PlantMarsh</thingClass>
		<defName>Plant_Mangrove</defName>
		<label>mangrove tree</label>
		<description>Mangroves are trees or shrubs that grow in salty water in hot places like the tropics.</description>
		<graphicData>
			<texPath>Things/Plant/PlantMangroveTree</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.15, 0.2, 0.15)</volume>
				<offset>(0,0,0)</offset>
			</shadowData>
		</graphicData>
		<statBases>
			<Beauty>2</Beauty>
		</statBases>
		<plant>
			<visualSizeRange>
				<min>1.5</min>
				<max>3.0</max>
			</visualSizeRange>
			<growDays>17</growDays>
			<harvestWork>900</harvestWork>
			<harvestYield>25</harvestYield>
			<wildClusterRadius>10</wildClusterRadius>
			<wildClusterWeight>11</wildClusterWeight>
		</plant>
	</ThingDef>

	<ThingDef ParentName="TreewoodBase">

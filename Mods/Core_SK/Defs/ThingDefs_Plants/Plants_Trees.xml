			</shadowData>
		</graphicData>
		<statBases>
			<Beauty>4</Beauty>
			<Nutrition>1.2</Nutrition>
		</statBases>
		<plant>
			<sowMinSkill>9</sowMinSkill>
			<sowResearchPrerequisites>
				<li>SK_OrchardsII</li>
			</sowResearchPrerequisites>

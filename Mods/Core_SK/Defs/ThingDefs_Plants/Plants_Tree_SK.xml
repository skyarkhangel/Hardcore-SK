<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!--============================ Wild  ============================-->

  <ThingDef ParentName="PlantBase">
    <defName>PlantRaspberry</defName>
    <label>Raspberry Bush</label>
    <description>Raspberry Bushes are plants that produce fragile, red, bumpy fruits called Raspberries.</description>
    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/Raspberry</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
        <volume>(0.6, 0.2, 0.6)</volume>
      </shadowData>
    </graphicData>
    <fillPercent>0.09</fillPercent>
    <selectable>true</selectable>
    <pathCost>30</pathCost>
    <ingestible>
      <preferability>NeverForNutrition</preferability>
      <foodType>Plant</foodType>
      <nutrition>0.25</nutrition>
    </ingestible>
    <plant>
      <leaflessGraphicPath>Things/Plant/Bush_Leafless</leaflessGraphicPath>
      <harvestWork>150</harvestWork>
      <harvestTag>Standard</harvestTag>
      <harvestDestroys>false</harvestDestroys>
      <harvestedThingDef>RawBerries</harvestedThingDef>
      <harvestYield>8</harvestYield>
      <fertilityMin>0.5</fertilityMin>
      <wildCommonalityMaxFraction>3.0</wildCommonalityMaxFraction>
      <wildClusterSizeRange>
        <min>2</min>
        <max>5</max>
      </wildClusterSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
      <visualSizeRange>
        <min>0.7</min>
        <max>1.1</max>
      </visualSizeRange>
      <growDays>6</growDays>
      <topWindExposure>0.3</topWindExposure>
      <fertilitySensitivity>0.5</fertilitySensitivity>
    </plant>
  </ThingDef>
 

  <ThingDef ParentName="PlantBase">
    <defName>PlantBush</defName>
    <label>Bush</label>
    <description>A Bush is a type of green, short shrubbery. Unlike other plants this one is incapable of producing fruits or vegetables.</description>
    <statBases>
      <MaxHitPoints>120</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/PlantBush</texPath>
      <graphicClass>Graphic_Random</graphicClass>
       <shadowData>
         <volume>(0.37, 0.2, 0.37)</volume>
       </shadowData>
    </graphicData>
    <fillPercent>0.09</fillPercent>
    <selectable>true</selectable>
    <pathCost>30</pathCost>
    <ingestible>
      <preferability>NeverForNutrition</preferability>
      <foodType>Plant</foodType>
      <nutrition>0.2</nutrition>
    </ingestible>
    <plant>
		<maxMeshCount>4</maxMeshCount>
      <leaflessGraphicPath>Things/Plant/Bush_Leafless</leaflessGraphicPath>
      <harvestWork>130</harvestWork>
      <fertilityMin>0.2</fertilityMin>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>Kindling</harvestedThingDef>
      <harvestDestroys>true</harvestDestroys>
      <harvestYield>4</harvestYield>
      <wildClusterSizeRange>
        <min>2</min>
        <max>5</max>
      </wildClusterSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
      <visualSizeRange>
        <min>0.7</min>
        <max>1.1</max>
      </visualSizeRange>
      <growDays>6</growDays>
      <topWindExposure>0.3</topWindExposure>
      <fertilitySensitivity>0.5</fertilitySensitivity>
      <sowTags>
        <li>Ground</li>
      </sowTags>
      <sowMinSkill>1</sowMinSkill>
      <sowResearchPrerequisites>
        <li>SK_TreesI</li>
      </sowResearchPrerequisites>
    </plant>
  </ThingDef>


  <ThingDef ParentName="PlantBase">
    <defName>PlantSaguaroCactus</defName>
    <label>Saguaro Cactus</label>
    <description>The Saguaro Cactus is native to desert and arid environments. When fully grown, it can be chopped for a small amount of wood.</description>
    <statBases>
      <MaxHitPoints>200</MaxHitPoints>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/SaguaroCactus</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
        <volume>(0.3, 0.3, 0.3)</volume>
      </shadowData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <selectable>true</selectable>
    <fillPercent>0.35</fillPercent>
    <passability>PassThroughOnly</passability>
    <pathCost>130</pathCost>
    <blockWind>true</blockWind>
    <ingestible>
      <nutrition>0.30</nutrition>
    </ingestible>
    <plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0</fertilitySensitivity>
			<growDays>5</growDays>
			<reproduceRadius>42</reproduceRadius>
			<leaflessGraphicPath>Things/Plant/SaguaroCactus_Leafless</leaflessGraphicPath>
			<soundHarvesting>Harvest_Tree</soundHarvesting>
			<soundHarvestFinish>Harvest_Tree_Finish</soundHarvestFinish>
			<harvestWork>300</harvestWork>
			<harvestDestroys>true</harvestDestroys>
			<harvestedThingDef>WoodLog</harvestedThingDef>
			<harvestTag>Wood</harvestTag>
			<harvestYield>10</harvestYield>
			<harvestMinGrowth>0.2</harvestMinGrowth>
			<harvestFailable>false</harvestFailable>
			<visualSizeRange>
				<min>1.3</min>
				<max>2.0</max>
			</visualSizeRange>
			<topWindExposure>0.08</topWindExposure>
    </plant>
  </ThingDef>


  <ThingDef ParentName="PlantBase">
    <defName>PlantPincushionCactus</defName>
    <label>Pincushion Cactus</label>
    <description>A Pincushion Cactus is a short, round cactus with long pin-like needles. It is native to desert and arid environments.</description>
    <statBases>
      <MaxHitPoints>150</MaxHitPoints>
      <Beauty>1</Beauty>
    </statBases>
    <graphicData>
      <texPath>Things/Plant/PincushionCactus</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <fillPercent>0.09</fillPercent>
    <selectable>true</selectable>
    <pathCost>30</pathCost>
    <hideAtSnowDepth>1</hideAtSnowDepth>
    <ingestible>
      <nutrition>0.15</nutrition>
    </ingestible>
    <plant>
      <fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0</fertilitySensitivity>
			<growDays>2.5</growDays>
			<reproduceRadius>42</reproduceRadius>
			<wildCommonalityMaxFraction>3.0</wildCommonalityMaxFraction>
			<visualSizeRange>
				<min>0.40</min>
				<max>0.65</max>
			</visualSizeRange>
			<topWindExposure>0.0</topWindExposure>
      <wildClusterSizeRange>
        <min>2</min>
        <max>5</max>
      </wildClusterSizeRange>
      <wildClusterRadius>4</wildClusterRadius>
      <maxMeshCount>4</maxMeshCount>
    </plant>
  </ThingDef>

  
  <ThingDef ParentName="TreewoodBase">
    <defName>PlantMangrove</defName>
    <label>Mangrove</label>
    <description>Mangroves are trees or shrubs that grow in salty water in hot places like the tropics.</description>
    <graphicData>
      <texPath>Things/Plant/MangroveTree</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <statBases>
      <Beauty>2</Beauty>
    </statBases>
    <plant>
      <visualSizeRange>
        <min>1.0</min>
        <max>2.5</max>
      </visualSizeRange>
      <growDays>30</growDays>
      <harvestWork>1500</harvestWork>
      <harvestYield>20</harvestYield>
    </plant>
  </ThingDef>
  
  <ThingDef ParentName="TreewoodBase">
    <defName>PlantWildBanana</defName>
    <label>Wild Coconut Tree</label>
    <description></description>
    <graphicData>
      <texPath>Things/Plant/WildBanana</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <Beauty>4</Beauty>
    </statBases>
    <plant>
      <visualSizeRange>
        <min>1.2</min>
        <max>3.4</max>
      </visualSizeRange>
      <growDays>13</growDays>
	  <fertilitySensitivity>1</fertilitySensitivity>
	  <reproduceMtbDays>3</reproduceMtbDays>
      <harvestWork>550</harvestWork>
      <harvestYield>30</harvestYield>
	  <harvestDestroys>false</harvestDestroys>
	  <harvestedThingDef>RawCoconut</harvestedThingDef>
	  <fertilityMin>0.03</fertilityMin>
    </plant>
  </ThingDef>
  
  <ThingDef ParentName="TreewoodBase">
    <defName>PlantTreeOak</defName>
    <label>Oak Tree</label>
    <description>Oak Trees are slow growing, strong, hardwood trees.</description>
    <graphicData>
      <texPath>Things/Trees/TreeOak</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <statBases>
      <Beauty>5</Beauty>
    </statBases>
    <plant>
      <visualSizeRange>
        <min>1.3</min>
        <max>3.0</max>
      </visualSizeRange>
      <leaflessGraphicPath>Things/Trees/TreeOak_Leafless</leaflessGraphicPath>
      <growDays>60</growDays>
      <harvestWork>1500</harvestWork>
      <harvestYield>75</harvestYield>
      <sowMinSkill>8</sowMinSkill>
      <sowResearchPrerequisites>
        <li>SK_TreesIII</li>
      </sowResearchPrerequisites>
    </plant>
  </ThingDef>

  <ThingDef ParentName="TreewoodBase">
    <defName>PlantTreePoplar</defName>
    <label>Poplar Tree</label>
    <description>Poplar Trees are quick growing, softwood trees. They produce less wood than Oak Trees.</description>
    <graphicData>
      <texPath>Things/Trees/TreePoplar</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <statBases>
      <Beauty>3</Beauty>
    </statBases>
    <ingestible>
      <nutrition>0.75</nutrition>
    </ingestible>
    <plant>
      <visualSizeRange>
        <min>1.2</min>
        <max>3.2</max>
      </visualSizeRange>
      <leaflessGraphicPath>Things/Trees/TreePoplar_Leafless</leaflessGraphicPath>
      <growDays>31</growDays>
      <harvestWork>650</harvestWork>
      <harvestYield>40</harvestYield>
      <sowMinSkill>3</sowMinSkill>
      <sowResearchPrerequisites>
        <li>SK_TreesII</li>
      </sowResearchPrerequisites>
    </plant>
  </ThingDef>


  <ThingDef ParentName="TreewoodBase">
    <defName>PlantTreePine</defName>
    <label>Pine Tree</label>
    <description>Pine Trees are large conifer, softwood trees.</description>
    <graphicData>
      <texPath>Things/Trees/TreePine</texPath>
      <graphicClass>Graphic_Random</graphicClass>
        <shadowData>
          <volume>(0.2, 0.3, 0.2)</volume>
        </shadowData>
    </graphicData>
    <statBases>
      <Beauty>5</Beauty>
    </statBases>
    <plant>
      <visualSizeRange>
        <min>1.3</min>

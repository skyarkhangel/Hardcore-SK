    <graphicData>
      <texPath>Things/Trees/TreeBirch</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shadowData>
        <volume>(0.15, 0.3, 0.1)</volume>
      </shadowData>
    </graphicData>
    <statBases>
      <Beauty>3</Beauty>
    </statBases>
    <plant>
      <visualSizeRange>
        <min>1.5</min>
        <max>2.9</max>
      </visualSizeRange>
      <leaflessGraphicPath>Things/Trees/TreeBirch_Leafless</leaflessGraphicPath>
      <growDays>40</growDays>

<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="PlantDef">
		<defName>Plant_Potato</defName>
		<label>Potato Plant</label>
		<description>Potato Plants produce nutritious tubers called Potatoes. Potato Plants grow quickly and are a reliable food source crop for basic meals.</description>
		<statBases>
			<Beauty>1</Beauty>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/PlantPotato_Harvest</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<plant>
			<maxMeshCount>4</maxMeshCount>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<immatureGraphicPath>Things/Plant/PlantPotato</immatureGraphicPath>
			<sowMinSkill>1</sowMinSkill>
			<harvestedThingDef>RawPotatoes</harvestedThingDef>
			<harvestYield>13</harvestYield>
			<topWindExposure>0.1</topWindExposure>
			<growDays>5</growDays>
			<fertilitySensitivity>0.4</fertilitySensitivity>
			<fertilityMin>0.5</fertilityMin>
			<visualSizeRange>
				<min>0.2</min>
				<max>1.0</max>
			</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>


	<ThingDef ParentName="SK_DeciduousPlantDef">
		<defName>Plant_Corn</defName>
		<label>Corn Plant</label>
		<description>Corn Plants are large grain plants that produce ears of delicious yellow seeds that are called Corn. Corn Plants grow slowly but yeild lots of food. Corn is useful as a food ingredient or for making Ethanol Fuel.</description>
		<graphicData>
			<texPath>Things/Plant/PlantCorn</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<statBases>
			<Nutrition>0.4</Nutrition>
		</statBases>
		<plant>
			<maxMeshCount>9</maxMeshCount>
			<sowMinSkill>6</sowMinSkill>
			<sowResearchPrerequisites>
				<li>SK_CerealsII</li>
			</sowResearchPrerequisites>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<immatureGraphicPath>Things/Plant/PlantCorn_Harvest</immatureGraphicPath>
			<harvestedThingDef>RawCorn</harvestedThingDef>
			<harvestYield>30</harvestYield>
			<topWindExposure>0.15</topWindExposure>
			<growDays>9</growDays>
			<fertilitySensitivity>1.4</fertilitySensitivity>
			<fertilityMin>0.4</fertilityMin>
			<visualSizeRange>
				<min>0.2</min>
				<max>1.0</max>
			</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

	<ThingDef ParentName="SK_DeciduousPlantDef">
		<defName>Plant_Rice</defName>
		<label>Rice Plant</label>
		<description>Rice Plants are short grain plants that yields numerous amounts of edible seeds called Rice. Rice is useful as a meal ingredient, can be made into Ethanol Fuel or distilled into Sake from the Brewery.</description>
		<graphicData>
			<texPath>Things/Plant/PlantRice_Harvest</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<plant>
			<sowMinSkill>12</sowMinSkill>
			<sowResearchPrerequisites>
				<li>SK_CerealsIII</li>
			</sowResearchPrerequisites>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<immatureGraphicPath>Things/Plant/PlantRice</immatureGraphicPath>
			<harvestedThingDef>RawRice</harvestedThingDef>
			
			<harvestTag>Standard</harvestTag>
			<harvestYield>16</harvestYield>
			<topWindExposure>0.1</topWindExposure>
			<growDays>7</growDays>
			<fertilitySensitivity>2.1</fertilitySensitivity>
			<fertilityMin>1.3</fertilityMin>
			<visualSizeRange>
				<min>0.2</min>
				<max>0.8</max>
			</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

	<ThingDef ParentName="PlantDef">
		<defName>Plantbean</defName>
		<label>Bean Stalk</label>
		<description>A Bean Stalk produces a Bean Pod, a soft shell with small, edible seeds inside. They can be used as a meal ingredient or can be refined into BioDiesel Fuel.</description>
		<graphicData>
			<texPath>Things/Plant/PlantBeans_Harvest</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<statBases>
			<Nutrition>0.4</Nutrition>
		</statBases>
		<plant>
			<maxMeshCount>4</maxMeshCount>
			<sowMinSkill>13</sowMinSkill>
			<sowResearchPrerequisites>
				<li>SK_VegetablesII</li>
			</sowResearchPrerequisites>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<immatureGraphicPath>Things/Plant/PlantBeans</immatureGraphicPath>
			<harvestedThingDef>Rawbean</harvestedThingDef>
			<harvestYield>30</harvestYield>
			<topWindExposure>0.1</topWindExposure>
			<growDays>9.2</growDays>
			<fertilitySensitivity>1</fertilitySensitivity>
			<fertilityMin>1</fertilityMin>
			<visualSizeRange>
				<min>0.20</min>
				<max>0.90</max>
			</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>


	<ThingDef ParentName="PlantDef">
		<defName>PlantTomato</defName>
		<label>Tomato Vine</label>
		<description>Tomato Vines produce round, palm sized, red, sharp-tasting fruits called Tomatoes. Tomatoes are related to Potato Plants and grow very quickly.</description>
		<graphicData>
			<texPath>Things/Plant/PlantTomato_Harvest</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<plant>
			<maxMeshCount>4</maxMeshCount>
			<sowMinSkill>10</sowMinSkill>
			<sowResearchPrerequisites>
				<li>SK_FruitsI</li>
			</sowResearchPrerequisites>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<immatureGraphicPath>Things/Plant/PlantTomato</immatureGraphicPath>
			<harvestedThingDef>RawTomatoes</harvestedThingDef>
			<harvestYield>8</harvestYield>
			<topWindExposure>0.1</topWindExposure>
			<growDays>7.2</growDays>
			<fertilitySensitivity>2.2</fertilitySensitivity>
			<fertilityMin>1.3</fertilityMin>
			<visualSizeRange>
				<min>0.2</min>
				<max>0.9</max>
			</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>


	<ThingDef ParentName="PlantDef">
		<defName>PlantCarrot</defName>
		<label>Carrot Plant</label>
		<description>Carrot Plants produce long, crunchy, orange root vegetables called Carrots. Carrots are refreshing to eat raw and have a long shelf life.</description>
		<graphicData>
			<texPath>Things/Plant/PlantCarrot_Harvest</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<statBases>
			<Nutrition>0.4</Nutrition>
		</statBases>
		<ingestible>
			<foodType>Plant</foodType>
			<preferability>NeverForNutrition</preferability>
		</ingestible>
		<plant>
			<maxMeshCount>9</maxMeshCount>
			<sowMinSkill>5</sowMinSkill>
			<sowResearchPrerequisites>
				<li>SK_VegetablesI</li>
			</sowResearchPrerequisites>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<immatureGraphicPath>Things/Plant/PlantCarrot</immatureGraphicPath>
			<harvestedThingDef>RawCarrots</harvestedThingDef>
			<harvestYield>9</harvestYield>
			<topWindExposure>0.1</topWindExposure>
			<growDays>5.4</growDays>
			<fertilitySensitivity>1</fertilitySensitivity>
			<fertilityMin>0.9</fertilityMin>
			<visualSizeRange>
				<min>0.10</min>
				<max>0.65</max>
			</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

	<ThingDef ParentName="PlantDef">
		<defName>Plant_Mushroom</defName>
		<label>Champignon Mushroom</label>
		<description>Champignon Mushrooms produce white or brown edible caps called Raw Champignon. This robust mushroom has been genetically engineered and is cultivated to produce a filling meat substitute.</description>
		<graphicData>
			<texPath>Things/Plant/PlantChampignon</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowDepth>0.2</hideAtSnowDepth>
		<plant>
			<maxMeshCount>9</maxMeshCount>
			<sowMinSkill>4</sowMinSkill>
			<sowResearchPrerequisites>
				<li>Fungiponics</li>
			</sowResearchPrerequisites>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<harvestedThingDef>Rawmushroom</harvestedThingDef>
			<harvestYield>12</harvestYield>
			<topWindExposure>0</topWindExposure>
			<growDays>5.1</growDays>
			<fertilitySensitivity>0.7</fertilitySensitivity>
			<fertilityMin>0.9</fertilityMin>
			<visualSizeRange>
				<min>0.05</min>
				<max>0.50</max>
			</visualSizeRange>
			<wildOrder>2</wildOrder>
			<purpose>Food</purpose>
		</plant>
	</ThingDef>

</Defs>

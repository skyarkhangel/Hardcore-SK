<?xml version="1.0" encoding="utf-8" ?>
<ThingDefs>

   <ThingDef ParentName="PlantDef">
    <defName>PlantPotato</defName>
    <label>Potato Plant</label>
    <description>Potato Plants produce nutritious tubers called Potatoes. Potato Plants grow quickly and are a reliable food source crop for basic meals.</description>
    <statBases>
      <Beauty>1</Beauty>
    </statBases>
  <graphicData>
    <texPath>Things/Plant/PlantPotato_Harvest</texPath>
	<graphicClass>Graphic_Random</graphicClass>
  </graphicData>
    <plant>
      <maxMeshCount>4</maxMeshCount>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
	  <immatureGraphicPath>Things/Plant/PlantPotato</immatureGraphicPath>
      <sowMinSkill>1</sowMinSkill>
      <harvestedThingDef>RawPotatoes</harvestedThingDef>
      <harvestYield>9</harvestYield>
      <topWindExposure>0.1</topWindExposure>
      <growDays>6.5</growDays>
      <fertilitySensitivity>0.4</fertilitySensitivity>
      <fertilityMin>0.5</fertilityMin>
      <visualSizeRange>
        <min>0.2</min>
        <max>1.0</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
  
  
   <ThingDef ParentName="PlantDef">
    <defName>PlantCorn</defName>
    <label>Corn Plant</label>
    <description>Corn Plants are large grain plants that produce ears of delicious yellow seeds that are called Corn. Corn Plants grow slowly but yeild lots of food. Corn is useful as a food ingredient or for making Ethanol Fuel.</description>
  <graphicData>
		<texPath>Things/Plant/PlantCorn</texPath>
		<graphicClass>Graphic_Random</graphicClass>
  </graphicData>
    <ingestible>
      <nutrition>0.40</nutrition>
    </ingestible>
    <plant>
      <maxMeshCount>9</maxMeshCount>
      <sowMinSkill>6</sowMinSkill>
      <sowResearchPrerequisites>
        <li>SK_CerealsII</li>
      </sowResearchPrerequisites>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
	  <immatureGraphicPath>Things/Plant/PlantCorn_Harvest</immatureGraphicPath>
      <harvestedThingDef>RawCorn</harvestedThingDef>
      <harvestYield>25</harvestYield>
      <topWindExposure>0.15</topWindExposure>
      <growDays>14.5</growDays>
      <fertilitySensitivity>1</fertilitySensitivity>
      <fertilityMin>0.4</fertilityMin>
      <visualSizeRange>
        <min>0.2</min>
        <max>1.0</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
  
   <ThingDef ParentName="PlantDef">
    <defName>PlantRice</defName>
    <label>Rice Plant</label>
    <description>Rice Plants are short grain plants that yields numerous amounts of edible seeds called Rice. Rice is useful as a meal ingredient, can be made into Ethanol Fuel or distilled into Sake from the Brewery.</description>
  <graphicData>
      <texPath>Things/Plant/PlantCorn_Harvest</texPath>
      <graphicClass>Graphic_Random</graphicClass>
  </graphicData>
    <plant>
      <sowMinSkill>12</sowMinSkill>
      <sowResearchPrerequisites>
        <li>SK_CerealsIII</li>
      </sowResearchPrerequisites>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
	  <immatureGraphicPath>Things/Plant/PlantRice</immatureGraphicPath>
      <harvestedThingDef>RawRice</harvestedThingDef>
	  <reproduces>false</reproduces>
	  <harvestTag>Standard</harvestTag>
      <harvestYield>10</harvestYield>
      <topWindExposure>0.1</topWindExposure>
      <growDays>5.2</growDays>
      <fertilitySensitivity>1</fertilitySensitivity>
      <fertilityMin>0.5</fertilityMin>
      <visualSizeRange>
        <min>0.2</min>
        <max>0.8</max>
      </visualSizeRange>
    </plant>
  </ThingDef>
  

<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef Name="SK_CavePlantBase" Class="SK.ThingDef_ClusterPlant" Abstract="True">
		<category>Plant</category>
		<thingClass>Plant</thingClass>
		<altitudeLayer>LowPlant</altitudeLayer>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<Flammability>0.8</Flammability>
		</statBases>
		<tickerType>Long</tickerType>
		<selectable>False</selectable>
		<neverMultiSelect>True</neverMultiSelect>
		<drawerType>MapMeshOnly</drawerType>
		<graphicData>
			<shaderType>CutoutPlant</shaderType>
		</graphicData>
		<plant>
			<soundHarvesting>Harvest_Standard</soundHarvesting>
			<soundHarvestFinish>Harvest_Standard_Finish</soundHarvestFinish>
			<harvestWork>150</harvestWork>
			<sowWork>150</sowWork>
			<fertilityMin>0.5</fertilityMin>
			<fertilitySensitivity>1.0</fertilitySensitivity>
			<topWindExposure>0.1</topWindExposure>
			<visualSizeRange>
				<min>0.3</min>
				<max>1.00</max>
			</visualSizeRange>
			<growMinGlow>0</growMinGlow>
			<wildOrder>1</wildOrder>
			<wildEqualLocalDistribution>false</wildEqualLocalDistribution>
			<neverBlightable>true</neverBlightable>
		</plant>
		<hideAtSnowDepth>0.4</hideAtSnowDepth>
	</ThingDef>

	<ThingDef ParentName="SK_CavePlantBase" Class="SK.ThingDef_ClusterPlant">
		<defName>Glowstool</defName>
		<label>glowbulb</label>
		<description>A small glowing bulb, growing in nearly every corner of dark caves. While being nutritious, its very rough taste make it better used as a cooked ingredient.</description>
		<thingClass>SK.ClusterPlant</thingClass>
		<statBases>
			<MaxHitPoints>65</MaxHitPoints>
			<Flammability>0.4</Flammability>
			<Beauty>11</Beauty>
			<Nutrition>0.2</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/PlantGlowbulb</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.3, 0.4, 0.3)</volume>
			</shadowData>
		</graphicData>
		<selectable>true</selectable>
		<neverMultiSelect>false</neverMultiSelect>
		<pathCost>15</pathCost>
		<ingestible>
			<foodType>Plant</foodType>
			<preferability>NeverForNutrition</preferability>
		</ingestible>
		<plant>
			<fertilityMin>0.01</fertilityMin>
			<fertilitySensitivity>0.15</fertilitySensitivity>
			<growDays>7</growDays>
			<sowTags>
				<li>Fungiponics</li>
			</sowTags>
			<sowResearchPrerequisites>
				<li>Fungiponics_B1</li>
			</sowResearchPrerequisites>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestMinGrowth>0.90</harvestMinGrowth>
			<harvestWork>50</harvestWork>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawGlowbulb</harvestedThingDef>
			<harvestYield>5</harvestYield>
			<neverBlightable>true</neverBlightable>
			<visualSizeRange>
				<min>0.6</min>
				<max>0.8</max>
			</visualSizeRange>
			<topWindExposure>0.02</topWindExposure>
			<wildClusterRadius>3</wildClusterRadius>
			<wildClusterWeight>5</wildClusterWeight>
			<cavePlantWeight>4.5</cavePlantWeight>
			<purpose>Food</purpose>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>4</glowRadius>
				<glowColor>(5,128,128,0)</glowColor>
			</li>
		</comps>

		<cavePlantCommonalityMaxFraction>0.3</cavePlantCommonalityMaxFraction>
		<minGrowTemperature>0</minGrowTemperature>
		<minOptimalGrowTemperature>10</minOptimalGrowTemperature>
		<maxOptimalGrowTemperature>40</maxOptimalGrowTemperature>
		<maxGrowTemperature>50</maxGrowTemperature>
		<hasStaticGlower>false</hasStaticGlower>
		<hasDynamicGlower>true</hasDynamicGlower>
		<growOnlyOnRoughRock>true</growOnlyOnRoughRock>
		<growOnlyUndeRoof>true</growOnlyUndeRoof>
		<growOnlyNearNaturalRock>true</growOnlyNearNaturalRock>
		<minFertility>0.0</minFertility>
		<maxFertility>999.0</maxFertility>
		<minLight>0.0</minLight>
		<maxLight>0.6</maxLight>
		<clusterSizeRange>
			<min>1</min>
			<max>4</max>
		</clusterSizeRange>
		<clusterSpawnRadius>3.5</clusterSpawnRadius>
		<clusterExclusivityRadiusOffset>5</clusterExclusivityRadiusOffset>
		<clusterExclusivityRadiusFactor>1</clusterExclusivityRadiusFactor>
		<isSymbiosisPlant>false</isSymbiosisPlant>
	</ThingDef>

	<ThingDef ParentName="SK_CavePlantBase" Class="SK.ThingDef_ClusterPlant">
		<defName>Agarilux</defName>
		<label>gleamcap</label>
		<description>A massive caveworld mushroom. Known for its natural healing properties by caveworld tenders.</description>
		<thingClass>SK.ClusterPlant_Gleamcap</thingClass>
		<statBases>
			<MaxHitPoints>90</MaxHitPoints>
			<Flammability>0.3</Flammability>
			<Beauty>15</Beauty>
			<Nutrition>0.4</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/PlantGleamcap</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.5, 0.5, 0.4)</volume>
			</shadowData>
		</graphicData>
		<selectable>true</selectable>
		<pathCost>18</pathCost>
		<fillPercent>0.20</fillPercent>
		<ingestible>
			<foodType>Plant</foodType>
			<preferability>NeverForNutrition</preferability>
		</ingestible>
		<plant>
			<fertilityMin>0.01</fertilityMin>
			<fertilitySensitivity>0.15</fertilitySensitivity>
			<growDays>11</growDays>
			<sowTags>
				<li>Fungiponics</li>
			</sowTags>
			<sowResearchPrerequisites>
				<li>Fungiponics_B1</li>
			</sowResearchPrerequisites>
			<sowMinSkill>8</sowMinSkill>
			<harvestMinGrowth>0.90</harvestMinGrowth>
			<harvestWork>100</harvestWork>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>GleamcapStem</harvestedThingDef>
			<harvestYield>2</harvestYield>
			<neverBlightable>true</neverBlightable>
			<visualSizeRange>
				<min>0.3</min>
				<max>1.2</max>
			</visualSizeRange>
			<topWindExposure>0.03</topWindExposure>
			<cavePlantWeight>3.5</cavePlantWeight>
			<wildClusterRadius>3</wildClusterRadius>
			<wildClusterWeight>5</wildClusterWeight>
			<purpose>Food</purpose>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>4</glowRadius>
				<glowColor>(5,128,85,0)</glowColor>
			</li>
		</comps>

		<cavePlantCommonalityMaxFraction>0.3</cavePlantCommonalityMaxFraction>
		<minGrowTemperature>0</minGrowTemperature>
		<minOptimalGrowTemperature>10</minOptimalGrowTemperature>
		<maxOptimalGrowTemperature>40</maxOptimalGrowTemperature>
		<maxGrowTemperature>50</maxGrowTemperature>
		<hasStaticGlower>false</hasStaticGlower>
		<hasDynamicGlower>true</hasDynamicGlower>
		<growOnlyOnRoughRock>true</growOnlyOnRoughRock>
		<growOnlyUndeRoof>true</growOnlyUndeRoof>
		<growOnlyNearNaturalRock>true</growOnlyNearNaturalRock>
		<minFertility>0.0</minFertility>
		<maxFertility>999.0</maxFertility>
		<minLight>0.0</minLight>
		<maxLight>0.6</maxLight>
		<clusterSizeRange>
			<min>1</min>
			<max>3</max>
		</clusterSizeRange>
		<clusterSpawnRadius>1.5</clusterSpawnRadius>
		<clusterExclusivityRadiusOffset>2</clusterExclusivityRadiusOffset>
		<clusterExclusivityRadiusFactor>2</clusterExclusivityRadiusFactor>
		<isSymbiosisPlant>false</isSymbiosisPlant>
	</ThingDef>

	<ThingDef ParentName="SK_CavePlantBase" Class="SK.ThingDef_ClusterPlant">
		<defName>Bryolux</defName>
		<label>shimmershroom</label>
		<description>Some nice and tasty caveworld shimmershrooms. Much appreciated for their delicate taste and nutrition qualities by caveworlds inhabitants.</description>
		<thingClass>SK.ClusterPlant</thingClass>
		<statBases>
			<MaxHitPoints>25</MaxHitPoints>
			<Flammability>0.4</Flammability>
			<Beauty>10</Beauty>
			<Nutrition>0.15</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/PlantShimmershroom</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.3, 0.3, 0.2)</volume>
			</shadowData>
		</graphicData>
		<selectable>true</selectable>
		<neverMultiSelect>false</neverMultiSelect>
		<pathCost>18</pathCost>
		<ingestible>
			<foodType>Plant</foodType>
			<preferability>NeverForNutrition</preferability>
		</ingestible>
		<plant>
			<fertilityMin>0.01</fertilityMin>
			<fertilitySensitivity>0.15</fertilitySensitivity>
			<growDays>6</growDays>
			<sowTags>
				<li>Fungiponics</li>
			</sowTags>
			<sowResearchPrerequisites>
				<li>Fungiponics_B1</li>
			</sowResearchPrerequisites>
			<sowMinSkill>7</sowMinSkill>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestMinGrowth>0.90</harvestMinGrowth>
			<harvestWork>50</harvestWork>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawShimmershroom</harvestedThingDef>
			<harvestYield>5</harvestYield>
			<neverBlightable>true</neverBlightable>
			<visualSizeRange>
				<min>0.6</min>
				<max>0.8</max>
			</visualSizeRange>
			<topWindExposure>0.03</topWindExposure>
			<cavePlantWeight>7.5</cavePlantWeight>
			<wildClusterRadius>3</wildClusterRadius>
			<wildClusterWeight>5</wildClusterWeight>
			<purpose>Food</purpose>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>4</glowRadius>
				<glowColor>(128,5,100,0)</glowColor>
			</li>
		</comps>

		<cavePlantCommonalityMaxFraction>0.6</cavePlantCommonalityMaxFraction>
		<minGrowTemperature>0</minGrowTemperature>
		<minOptimalGrowTemperature>10</minOptimalGrowTemperature>
		<maxOptimalGrowTemperature>40</maxOptimalGrowTemperature>
		<maxGrowTemperature>50</maxGrowTemperature>
		<hasStaticGlower>false</hasStaticGlower>
		<hasDynamicGlower>true</hasDynamicGlower>
		<growOnlyOnRoughRock>true</growOnlyOnRoughRock>
		<growOnlyUndeRoof>true</growOnlyUndeRoof>
		<growOnlyNearNaturalRock>true</growOnlyNearNaturalRock>
		<minFertility>0.0</minFertility>
		<maxFertility>999.0</maxFertility>
		<minLight>0.0</minLight>
		<maxLight>0.6</maxLight>
		<clusterSizeRange>
			<min>4</min>
			<max>7</max>
		</clusterSizeRange>
		<clusterSpawnRadius>2.0</clusterSpawnRadius>
		<clusterExclusivityRadiusOffset>5</clusterExclusivityRadiusOffset>
		<clusterExclusivityRadiusFactor>2</clusterExclusivityRadiusFactor>
		<isSymbiosisPlant>false</isSymbiosisPlant>
	</ThingDef>

	<ThingDef ParentName="SK_CavePlantBase" Class="SK.ThingDef_ClusterPlant">
		<defName>Brightbells</defName>
		<label>brightbells</label>
		<description>A very elegant fungus, appreciated by miners. Remarkable by the light emitted by the little bulbs ending its fragile branches. It is often used by caveworld people as a natural lightning. Its fibrous texture renders it inedible though.</description>
		<thingClass>SK.ClusterPlant</thingClass>
		<statBases>
			<MaxHitPoints>15</MaxHitPoints>
			<Flammability>0.3</Flammability>
			<Beauty>35</Beauty>
			<Nutrition>0.10</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Flowers/SK/brightbells</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.3, 0.3, 0.3)</volume>
			</shadowData>
		</graphicData>
		<altitudeLayer>Item</altitudeLayer>
		<selectable>true</selectable>
		<fillPercent>0.20</fillPercent>
		<pathCost>15</pathCost>
		<ingestible>
			<foodType>Plant</foodType>
			<preferability>NeverForNutrition</preferability>
		</ingestible>
		<plant>
			<fertilityMin>0.01</fertilityMin>
			<fertilitySensitivity>0.15</fertilitySensitivity>
			<topWindExposure>0</topWindExposure>
			<harvestMinGrowth>0.90</harvestMinGrowth>
			<harvestWork>100</harvestWork>
			<harvestTag>Standard</harvestTag>
			<visualSizeRange>
				<min>0.3</min>
				<max>1.05</max>
			</visualSizeRange>
			<growDays>2.5</growDays>
			<sowTags>
				<li>Decorative</li>
			</sowTags>
			<sowResearchPrerequisites>
				<li>Fungiponics_A1</li>
			</sowResearchPrerequisites>
			<neverBlightable>true</neverBlightable>
			<cavePlantWeight>6.5</cavePlantWeight>
			<wildClusterRadius>3</wildClusterRadius>
			<wildClusterWeight>5</wildClusterWeight>
			<purpose>Food</purpose>
		</plant>

		<cavePlantCommonalityMaxFraction>0.2</cavePlantCommonalityMaxFraction>
		<minGrowTemperature>0</minGrowTemperature>
		<minOptimalGrowTemperature>10</minOptimalGrowTemperature>
		<maxOptimalGrowTemperature>40</maxOptimalGrowTemperature>
		<maxGrowTemperature>50</maxGrowTemperature>
		<hasStaticGlower>false</hasStaticGlower>
		<hasDynamicGlower>true</hasDynamicGlower>
		<growOnlyOnRoughRock>true</growOnlyOnRoughRock>
		<growOnlyUndeRoof>true</growOnlyUndeRoof>
		<growOnlyNearNaturalRock>true</growOnlyNearNaturalRock>
		<isFlower>true</isFlower>
		<minFertility>0.0</minFertility>
		<maxFertility>999.0</maxFertility>
		<minLight>0.0</minLight>
		<maxLight>0.6</maxLight>
		<clusterSizeRange>
			<min>1</min>
			<max>1</max>
		</clusterSizeRange>
		<clusterSpawnRadius>4.0</clusterSpawnRadius>
		<clusterExclusivityRadiusOffset>16</clusterExclusivityRadiusOffset>
		<clusterExclusivityRadiusFactor>0</clusterExclusivityRadiusFactor>
		<isSymbiosisPlant>false</isSymbiosisPlant>
		<modExtensions>
			<li Class="SK.ExtendedPlantGraphic">
				<flowerPotGraphic>Things/Plant/FlowerPotGraphics/Brightbells</flowerPotGraphic>
			</li>
		</modExtensions>
	</ThingDef>

</Defs>

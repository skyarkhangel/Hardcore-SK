	<!-- lifesupport system -->
   <ThingDef ParentName="BodyPartBase">
      <defName>lifesupport</defName>
      <label>Life Support System</label>
      <description>A basic piece of technology made to keep the victim/patient alive.</description>
      <graphicData>

	<!-- ================= Carpet ================= -->


	<TerrainDef Name="CarpetSK_FloorBase" ParentName="SK_FloorBase" Abstract="True">
		<designationCategory>Floors</designationCategory>
		<constructEffect>ConstructDirt</constructEffect>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<pathCost>0</pathCost>
		<fertility>0</fertility>
		<statBases>
			<WorkToBuild>170</WorkToBuild>
			<Beauty>4</Beauty>
			<Flammability>0.6</Flammability>
			<CleaningTimeFactor>2</CleaningTimeFactor>
		</statBases>
		<costList>
			<Cloth>15</Cloth>
		</costList>
		<burnedDef>BurnedCarpet</burnedDef>
		<!--<designatorDropdown>Floor_Carpet</designatorDropdown>-->
		<tags>
			<li>FineFloor</li>
		</tags>
		<ignoreIllegalLabelCharacterConfigError>true</ignoreIllegalLabelCharacterConfigError>
	</TerrainDef>

	<TerrainDef Name="SK_CarpetBase" ParentName="CarpetSK_FloorBase" Abstract="True">
		<texturePath>Terrain/Surfaces/Carpet</texturePath>
		<researchPrerequisites>
			<li>Carpets_B2</li>
		</researchPrerequisites>
	</TerrainDef>

	<TerrainDef ParentName="SK_CarpetBase">
		<defName>CarpetBeige</defName>
		<label>beige carpet</label>
		<renderPrecedence>197</renderPrecedence>
		<description>Stunning beige carpet that is soft and relaxing.</description>
		<texturePath>Terrain/Surfaces/CarpetBeige</texturePath>
	</TerrainDef>	

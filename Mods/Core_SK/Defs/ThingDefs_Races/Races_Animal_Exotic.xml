<Defs>
	<!-- Feenix -->
	<Verse.ThingDef ParentName="SK_AnimalThingBase">
		<defName>Feenix</defName>
		<label>Feenix</label>
		<description>Said to be fire incarnate, a single Feenix can lay waste to an entire colony. The leave a trail of flames wherever they go. Their hearts will continue to pulse with heat even after the creature has died.</description>
		<tickerType>Normal</tickerType>
		<statBases>
			<Mass>130</Mass>
			<Flammability>0</Flammability>
			<MoveSpeed>7.0</MoveSpeed>
			<LeatherAmount>75</LeatherAmount>
			<ComfyTemperatureMin>-120</ComfyTemperatureMin>
			<ComfyTemperatureMax>99999</ComfyTemperatureMax>
			<MarketValue>15000</MarketValue>
			<ArmorRating_Blunt>12</ArmorRating_Blunt>
			<ArmorRating_Sharp>4</ArmorRating_Sharp>
			<MeleeDodgeChance>0.05</MeleeDodgeChance>
			<MeleeCritChance>0</MeleeCritChance>
			<GermResistance>0.45</GermResistance>
			<GermContainment>0.15</GermContainment>
		</statBases>
		<verbs>
			<li Class="CombatExtended.VerbPropertiesCE">
				<recoilAmount>0.95</recoilAmount>
				<verbClass>Verb_Shoot</verbClass>
				<accuracyTouch>1</accuracyTouch>
				<accuracyShort>1</accuracyShort>
				<accuracyMedium>1</accuracyMedium>
				<accuracyLong>1</accuracyLong>
				<hasStandardCommand>true</hasStandardCommand>
				<defaultProjectile>DragonFireBreathOmega</defaultProjectile>
				<warmupTime>3</warmupTime>
				<burstShotCount>18</burstShotCount>
				<ticksBetweenBurstShots>3</ticksBetweenBurstShots>
				<minRange>4</minRange>
				<range>17</range>
				<soundCast>FlameThrower</soundCast>
				<muzzleFlashScale>0</muzzleFlashScale>
				<!--Commanility determines how frequent this range verb is chosen.-->
				<commonality>0.5</commonality>
			</li>
		</verbs>
		<tools>
			<li Class="CombatExtended.ToolCE">
				<label>Left claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>26</power>
				<cooldownTime>1.9</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftClaws</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>21</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
				<armorPenetrationSharp>5</armorPenetrationSharp>
				<armorPenetrationBlunt>15</armorPenetrationBlunt>
			</li>
			<li Class="CombatExtended.ToolCE">
				<label>Right claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>26</power>
				<cooldownTime>1.9</cooldownTime>
				<linkedBodyPartsGroup>FrontRightClaws</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>21</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
				<armorPenetrationSharp>5</armorPenetrationSharp>
				<armorPenetrationBlunt>15</armorPenetrationBlunt>
			</li>
			<li Class="CombatExtended.ToolCE">
				<capacities>
					<li>Flame</li>
				</capacities>
				<power>32</power>
				<cooldownTime>1.7</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.7</chanceFactor>
				<armorPenetrationSharp>8</armorPenetrationSharp>
				<armorPenetrationBlunt>13</armorPenetrationBlunt>
			</li>
			<li Class="CombatExtended.ToolCE">
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>31</power>
				<cooldownTime>3</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
				<armorPenetrationBlunt>12</armorPenetrationBlunt>
			</li>
		</tools>
		<comps>
			<li Class="CompProperties_EggLayer">
				<eggFertilizedDef>EggFenixFertilized</eggFertilizedDef>
				<eggFertilizationCountMax>1</eggFertilizationCountMax>
				<eggLayIntervalDays>60</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
				<eggCountRange>
					<min>1</min>
					<max>1</max>
				</eggCountRange>
			</li>
			<li Class="SK.CompProperties_Regeneration">
				<rateInTicks>250</rateInTicks> <!-- frequency in ticks -->
				<regenerationType>1</regenerationType> <!-- 1, 2 -->
			</li>
			<li Class="SK.CompProperties_Phoenix">
				<rateInTicks>250</rateInTicks> <!-- frequency in ticks -->
			</li>
		</comps>
		<race>
			<predator>true</predator>
			<maxPreyBodySize>1.5</maxPreyBodySize>
			<herdMigrationAllowed>false</herdMigrationAllowed>
			<executionRange>5</executionRange>
			<nameGenerator>NamerAnimalGenericMale</nameGenerator>
			<nameGeneratorFemale>NamerAnimalGenericFemale</nameGeneratorFemale>
			<hediffGiverSets>
				<li>OrganicStandard</li>
				<li>UnstoppablePassiveSet</li>
			</hediffGiverSets>
			<body>QuadrupedAnimalWithClawsTailAndJowlAndHeartGroup</body>
			<petness>0.15</petness>
			<baseBodySize>1.6</baseBodySize>
			<baseHungerRate>1.5</baseHungerRate>
			<baseHealthScale>9</baseHealthScale>
			<foodType>OmnivoreAnimal, OvivoreAnimal, CarnivoreAnimal, CarnivoreAnimalStrict</foodType>
			<useMeatFrom>Muffalo</useMeatFrom>
			<leatherDef>Leather_FireDragonskin</leatherDef>
			<trainability>Advanced</trainability>
			<gestationPeriodDays>70</gestationPeriodDays>
			<wildness>0.81</wildness>
			<manhunterOnTameFailChance>0.005</manhunterOnTameFailChance>
			<manhunterOnDamageChance>0.3</manhunterOnDamageChance>
			<lifeExpectancy>150</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>5</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>20</minAge>
					<soundWounded>Lion_Hurt</soundWounded>
					<soundDeath>Pawn_Lion_Death</soundDeath>
					<soundCall>Pawn_Lion_Call</soundCall>
					<soundAngry>Pawn_Lion_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Interact_Ignite</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Interact_Ignite</soundMeleeHitBuilding>
			<soundMeleeMiss>Interact_Ignite</soundMeleeMiss>
		</race>
		<modExtensions>
			<li Class="CombatExtended.RacePropertiesExtensionCE">
				<bodyShape>Quadruped</bodyShape>
			</li>
			<li Class="SK.FireAnimalExtension"/>
		</modExtensions>
	</Verse.ThingDef>


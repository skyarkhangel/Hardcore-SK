	<Verse.ThingDef ParentName="SK_AnimalThingBase">
		<defName>Terramorph</defName>
		<label>terramorph</label>
		<description>Terramorphs are a type of large beetle with a pair of sharp tusks, thick chitin plating and a toxic bite. They are well adapted to survive in extreme temperatures, be it glacial environments or scorching deserts.</description>
		<statBases>
			<Mass>170</Mass>
			<MoveSpeed>2.4</MoveSpeed>
			<ArmorRating_Blunt>7</ArmorRating_Blunt>
			<ArmorRating_Sharp>2.6</ArmorRating_Sharp>
			<ComfyTemperatureMin>-90</ComfyTemperatureMin>
			<ComfyTemperatureMax>80</ComfyTemperatureMax>
			<LeatherAmount>0</LeatherAmount>
			<MarketValue>3500</MarketValue>
			<ImmunityGainSpeed>2.5</ImmunityGainSpeed>
			<MeatAmount>75</MeatAmount>
			<MeleeDodgeChance>0.1</MeleeDodgeChance>
			<MeleeCritChance>0.15</MeleeCritChance>
			<GermResistance>0.33</GermResistance>
			<GermContainment>0.05</GermContainment>
		</statBases>
		<tools>
			<li Class="CombatExtended.ToolCE">
				<capacities>
					<li>ToxicBite</li>
				</capacities>
				<power>16</power>
				<cooldownTime>2.1</cooldownTime>
				<linkedBodyPartsGroup>HeadClaw</linkedBodyPartsGroup>
				<armorPenetrationSharp>4</armorPenetrationSharp>
				<armorPenetrationBlunt>7.8</armorPenetrationBlunt>
			</li>
			<li Class="CombatExtended.ToolCE">
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>11</power>
				<cooldownTime>1.95</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
				<armorPenetrationBlunt>8</armorPenetrationBlunt>
			</li>
		</tools>
		<comps>
			<li Class="SK.CompProperties_ShearableRenameable">
				<compClass>SK.CompShearableRenameable</compClass>
				<growthLabel>Chitin</growthLabel>
				<woolDef>Chitin</woolDef>
				<shearIntervalDays>10</shearIntervalDays>
				<woolAmount>24</woolAmount>
			</li>
		</comps>
		<butcherProducts>
			<Chitin>5</Chitin>
		</butcherProducts>
		<race>
			<predator>true</predator>
			<baseHungerRate>0.4</baseHungerRate>
			<body>BeetleLikeWithClaw</body>
			<baseBodySize>1.2</baseBodySize>
			<baseHealthScale>2.1</baseHealthScale>
			<petness>0</petness>
			<foodType>CarnivoreAnimal, Corpse, AnimalProduct, OvivoreAnimal, OmnivoreAnimal</foodType>
			<fleshType>Insectoid</fleshType>
			<bloodDef>Filth_BloodInsect</bloodDef>
			<useMeatFrom>Megaspider</useMeatFrom>
			<wildness>0.95</wildness>
			<manhunterOnTameFailChance>0.01</manhunterOnTameFailChance>
			<manhunterOnDamageChance>0.3</manhunterOnDamageChance>
			<lifeExpectancy>10</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>EusocialInsectLarva</def>

			<MeleeCritChance>0</MeleeCritChance>
		</statBases>
		<tools>
			<li Class="CombatExtended.ToolCE">
				<capacities>
					<li>ToxicBite</li>
				</capacities>
				<power>10</power>
				<cooldownTime>1.65</cooldownTime>
				<linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>

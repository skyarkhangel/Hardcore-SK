<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--=============== Antimatter ====================-->


	<ThingDef ParentName="SK_ResourceBase">
		<defName>Antimatter</defName>
		<label>AntiMatter</label>
		<description>AntiMatter in the form of anti-atoms is one of the most difficult materials to produce. AntiMatter in the form of individual anti-particles however, is commonly produced by particle accelerators and in some types of radioactive decay.</description>
		<graphicData>
			<texPath>Things/Building/Production/HadronCollider/Antimatter</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stackLimit>75</stackLimit>
		<tradeability>None</tradeability>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Metal_Drop</soundDrop>
		<techLevel>Ultra</techLevel>
		<statBases>
			<MaxHitPoints>5</MaxHitPoints>
			<MarketValue>120</MarketValue>
			<DeteriorationRate>0.01</DeteriorationRate>
			<Flammability>5</Flammability>
			<MaxBurningTempCelsius>10000</MaxBurningTempCelsius>
			<BurnDurationHours>12</BurnDurationHours>
			<Mass>0.01</Mass>
			<Bulk>0.1</Bulk>
		</statBases>
		<tickerType>Normal</tickerType>
		<comps>
			<li Class="CompProperties_Explosive">
				<explosiveRadius>1.5</explosiveRadius>
				<explosiveDamageType>Bomb</explosiveDamageType>
				<explosiveExpandPerStackcount>0.5</explosiveExpandPerStackcount>
				<startWickHitPointsPercent>0.333</startWickHitPointsPercent>
				<wickTicks>
					<min>5</min>
					<max>10</max>
				</wickTicks>
			</li>
		</comps>
		<thingCategories>
			<li>SuperMaterialCat</li>
		</thingCategories>
	</ThingDef>


	<!--=============== Magnetic material ====================-->


	<ThingDef ParentName="SK_ResourceBase">
		<defName>MagneticMaterial</defName>
		<label>Magnetic Material</label>
		<description>An advanced combination of strong magnetic materials. Creation of such material is beyond the fabrication skills of anyone but an electromagnetic physicist.</description>
		<graphicData>
			<texPath>Things/Item/Resource/MagneticMaterial</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stackLimit>75</stackLimit>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Metal_Drop</soundDrop>
		<tickerType>Rare</tickerType>
		<techLevel>Spacer</techLevel>
		<statBases>
			<MarketValue>110</MarketValue>
			<DeteriorationRate>0</DeteriorationRate>
			<Flammability>0.1</Flammability>
			<Mass>0.05</Mass>
			<Bulk>0.1</Bulk>
		</statBases>
		<thingCategories>
			<li>SuperMaterialCat</li>
		</thingCategories>
	</ThingDef>

	<!--=============== Biosynthetic material ====================-->


	<ThingDef ParentName="SK_ResourceBase">
		<defName>BiosyntheticMaterial</defName>
		<label>BioSynthetic Material</label>
		<description>BioSynthetic Material is an advanced, high ductility material.</description>
		<graphicData>
			<texPath>Things/Item/Resource/BiosyntheticMaterial</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<stackLimit>75</stackLimit>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Metal_Drop</soundDrop>
		<tickerType>Rare</tickerType>
		<techLevel>Spacer</techLevel>
		<statBases>
			<MarketValue>90</MarketValue>
			<DeteriorationRate>0</DeteriorationRate>
			<Flammability>0.3</Flammability>
			<Mass>0.07</Mass>
			<Bulk>0.1</Bulk>
		</statBases>
		<thingCategories>
			<li>SuperMaterialCat</li>
		</thingCategories>
	</ThingDef>

	<ThingDef ParentName="SK_ResourceBase">
		<defName>Hexcell</defName>
		<label>HexCell</label>
		<description>A compact energy storing device. It is generally used for powering advanced weaponry or shock absorbers in powered armor.</description>
		<graphicData>
			<texPath>Things/Item/HexCell</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stackLimit>50</stackLimit>
		<useHitPoints>true</useHitPoints>
		<techLevel>Spacer</techLevel>
		<statBases>
			<MaxHitPoints>10</MaxHitPoints>
			<MarketValue>150</MarketValue>
			<DeteriorationRate>0.02</DeteriorationRate>
			<Flammability>5</Flammability>
			<Mass>0.1</Mass>
			<Bulk>0.1</Bulk>
		</statBases>
		<tickerType>Normal</tickerType>
		<comps>
			<li Class="CompProperties_Explosive">
				<explosiveRadius>1.4</explosiveRadius>
				<explosiveDamageType>Flame</explosiveDamageType>
				<explosiveExpandPerStackcount>0.2</explosiveExpandPerStackcount>
				<startWickHitPointsPercent>0.333</startWickHitPointsPercent>
				<wickTicks>
					<min>5</min>
					<max>10</max>
				</wickTicks>
			</li>
		</comps>
		<thingCategories>
			<li>CTextiles</li>
		</thingCategories>
	</ThingDef>

</Defs>
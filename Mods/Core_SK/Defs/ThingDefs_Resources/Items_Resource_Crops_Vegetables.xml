<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="SK_PlantFoodRawBase">
		<defName>RawPotatoes</defName>
		<label>Potatoes</label>
		<description>Potatoes are a starchy, tuberous crop of a Potato Plant. They are not very tasty when eaten raw but they do grow quickly.</description>
		<graphicData>
			<texPath>Things/Veg/Potato</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.9</MarketValue>
			<Nutrition>0.07</Nutrition>
		</statBases>
		<tickerType>Rare</tickerType>
		<thingCategories>
			<li>BasicPlantFoodRaw</li>
		</thingCategories>
		<ingestible>
			<foodType>VegetableOrFruit</foodType>
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>30</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
	</ThingDef>


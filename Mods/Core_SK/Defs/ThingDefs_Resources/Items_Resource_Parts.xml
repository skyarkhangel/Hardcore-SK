	<ThingDef ParentName="CropsBase">
		<defName>ReinforcedConcrete</defName>
		<label>reinforced concrete</label>
		<description>Reinforced concrete is made from concrete and metal in order to strengthen the mixture.</description>
		<graphicData>
			<texPath>Things/Item/Parts/ReinforcedConcrete</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Metal_Drop</soundDrop>
		<statBases>
			<MarketValue>16</MarketValue>
			<SharpDamageMultiplier>1.3</SharpDamageMultiplier>
			<BluntDamageMultiplier>1.3</BluntDamageMultiplier>
			<DeteriorationRate>0</DeteriorationRate>
			<Flammability>0</Flammability>
		</statBases>
		<useHitPoints>true</useHitPoints>
		<thingCategories>
			<li>ConstructionParts</li>
		</thingCategories>
		<stuffProps>
			<categories>
				<li>Stony</li>
			</categories>
			<stuffAdjective>Reinforced Concrete</stuffAdjective>
			<appearance>Planks</appearance>
			<color>(122,122,120)</color>
			<constructEffect>ConstructMetal</constructEffect>
			<soundImpactStuff>BulletImpactGround</soundImpactStuff>
			<soundMeleeHitSharp>MeleeHit_Metal_Sharp</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Metal_Blunt</soundMeleeHitBlunt>
			<statFactors>
				<MaxHitPoints>2.8</MaxHitPoints>
				<Beauty>0</Beauty>
				<Flammability>0</Flammability>
				<WorkToMake>4.5</WorkToMake>
				<WorkToBuild>4.5</WorkToBuild>
				<DoorOpenSpeed>0.45</DoorOpenSpeed>
				<BedRestEffectiveness>0.9</BedRestEffectiveness>
				<MeleeWeapon_CooldownMultiplier>1.3</MeleeWeapon_CooldownMultiplier>
				<MeleePenetrationFactor>1.1</MeleePenetrationFactor>
				<Mass>1.6</Mass>
			</statFactors>
		</stuffProps>
		<stackLimit>75</stackLimit>
	</ThingDef>



	<ThingDef ParentName="SK_ResourceBase">
		<defName>Polymers</defName>
		<label>Polymers</label>
		<description>Polymers are a product of refined Crude Oil. It is a useful raw material used in creation of Plastic, Rubber, Synthetic Fibers and Carbon at a Petrochemical Plant.</description>
		<graphicData>
			<texPath>Things/Item/Resource/Polymers</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(255,255,255)</color>
		</graphicData>
		<soundInteract>Standard_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<tickerType>Rare</tickerType>
		<techLevel>Industrial</techLevel>
		<statBases>
			<MarketValue>1.5</MarketValue>
			<DeteriorationRate>0</DeteriorationRate>
			<Flammability>5</Flammability>
			<Mass>0.07</Mass>
			<Bulk>0.07</Bulk>
		</statBases>
		<thingCategories>
			<li>Hydrocarbons</li>
		</thingCategories>
		<stackLimit>600</stackLimit>
	</ThingDef>


	<ThingDef ParentName="SK_ResourceBase">
		<defName>Plastic</defName>
		<label>Plastic</label>
		<description>Plastic is a useful raw material for building construction or item production or weapons.</description>
		<graphicData>
			<texPath>Things/Item/Resource/Plastic</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Standard_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<tickerType>Rare</tickerType>
		<techLevel>Industrial</techLevel>
		<statBases>
			<MarketValue>9</MarketValue>
			<StuffPower_Armor_Blunt>0.5</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Sharp>0.4</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Heat>0.04</StuffPower_Armor_Heat>
			<StuffPower_Insulation_Cold>1.1</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>0.5</StuffPower_Insulation_Heat>
			<DeteriorationRate>0</DeteriorationRate>
			<Flammability>2</Flammability>
			<Mass>0.05</Mass>
			<Bulk>0.05</Bulk>
		</statBases>
		<thingCategories>
			<li>Chemical</li>
		</thingCategories>
		<stuffProps>
			<categories>
				<li>Plastic</li>
			</categories>
			<constructEffect>ConstructMetal</constructEffect>
			<color>(100,60,100)</color>
			<soundImpactStuff>BulletImpact_Metal</soundImpactStuff>
			<statFactors>
				<MaxHitPoints>1.1</MaxHitPoints>
				<Flammability>1</Flammability>
				<Beauty>1.3</Beauty>
				<WorkToMake>1.3</WorkToMake>
				<WorkToBuild>1.3</WorkToBuild>
				<DoorOpenSpeed>1.5</DoorOpenSpeed>
				<MeleeWeapon_CooldownMultiplier>0.7</MeleeWeapon_CooldownMultiplier>
				<MoveSpeed>1.3</MoveSpeed>
				<MarketValue>0.7</MarketValue>
				<BedRestEffectiveness>1.45</BedRestEffectiveness>
				<ImmunityGainSpeedFactor>1.35</ImmunityGainSpeedFactor>
				<WorkTableWorkSpeedFactor>1.2</WorkTableWorkSpeedFactor>
				<MeleePenetrationFactor>0.3</MeleePenetrationFactor>
				<Mass>0.5</Mass>
			</statFactors>
		</stuffProps>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<comps>
			<li>
				<compClass>CompColorable</compClass>
			</li>
		</comps>
		<stackLimit>300</stackLimit>
	</ThingDef>



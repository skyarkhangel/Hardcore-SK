	<ThingDef ParentName="SK_ResourceBase">
		<defName>CopperBar</defName>
		<label>copper alloy</label>
		<description>Copper is an alloy of Copper Ore. It is mainly used for its high conductivity in electrical applications. Copper is not a magnetic metal.</description>
		<graphicData>
			<texPath>Things/Item/Ingot</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
			<color>(160,115,45)</color>
		</graphicData>
		<soundInteract>Metal_Drop</soundInteract>
		<soundDrop>Metal_Drop</soundDrop>
		<techLevel>Medieval</techLevel>
		<statBases>
			<MarketValue>3</MarketValue>
			<StuffPower_Armor_Blunt>1.0</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Sharp>0.6</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Heat>0.06</StuffPower_Armor_Heat>
			<StuffPower_Insulation_Cold>-5</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>-5</StuffPower_Insulation_Heat>
			<SharpDamageMultiplier>0.5</SharpDamageMultiplier>
			<BluntDamageMultiplier>0.7</BluntDamageMultiplier>
			<DeteriorationRate>0</DeteriorationRate>
			<Flammability>0</Flammability>
			<Mass>0.4</Mass>
			<Bulk>0.4</Bulk>
		</statBases>
		<useHitPoints>true</useHitPoints>
		<thingCategories>
			<li>HCMBar</li>
			<li>HeavyBar</li>
		</thingCategories>
		<stackLimit>300</stackLimit>
		<smeltable>true</smeltable>
		<stuffProps>
			<stuffAdjective>copper</stuffAdjective>
			<categories>
				<li>Metallic</li>
				<li>StrongMetallic</li>
			</categories>
			<constructEffect>ConstructMetal</constructEffect>
			<color>(160,115,45)</color>
			<commonality>1.0</commonality>			
			<soundImpactStuff>BulletImpact_Metal</soundImpactStuff>
			<soundMeleeHitSharp>MeleeHit_Metal_Sharp</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Metal_Blunt</soundMeleeHitBlunt>
			<statOffsets>
				<Beauty>4</Beauty>
			</statOffsets>
			<statFactors>
				<MaxHitPoints>1.4</MaxHitPoints>
				<Flammability>0.1</Flammability>
				<Beauty>1.2</Beauty>
				<WorkToMake>1.5</WorkToMake>
				<WorkToBuild>1.5</WorkToBuild>
				<MeleeWeapon_CooldownMultiplier>1.3</MeleeWeapon_CooldownMultiplier>
				<MoveSpeed>1.0</MoveSpeed>
				<MarketValue>0.7</MarketValue>
				<BedRestEffectiveness>0.8</BedRestEffectiveness>
				<ImmunityGainSpeedFactor>1.25</ImmunityGainSpeedFactor>
				<WorkTableWorkSpeedFactor>0.85</WorkTableWorkSpeedFactor>
				<DoorOpenSpeed>0.7</DoorOpenSpeed>

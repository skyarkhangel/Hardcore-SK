<ThingDefs>


    <ThingDef ParentName="PlantFoodRawBase">
    <defName>RawCorn</defName>
    <label>Corn</label>
    <description>Corn comes from the Corn Plant. The leafy stalk of the plant produces ears which contain the grain. Corn can be cooked for food or made into Ethanol Fuel. Corn is also known as Maize.</description>
    <soundInteract>Grain_Drop</soundInteract>
    <soundDrop>Grain_Drop</soundDrop>
    <graphicData>
      <texPath>Things/Veg/Corn</texPath>
    </graphicData>
    <statBases>
      <MarketValue>2.1</MarketValue>
    </statBases>
    <tickerType>Rare</tickerType>   
	<thingCategories>
      <li>BasicPlantFoodRaw</li>
    </thingCategories>
    <ingestible>
      <foodType>VegetableOrFruit</foodType>
      <nutrition>0.04</nutrition>
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>60</daysToRotStart>
      </li>
    </comps>
  </ThingDef>

  

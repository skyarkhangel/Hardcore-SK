<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerMeleeWeapon</defName>
    <include>
      <li>NamerWeapon</li>
    </include>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerGun</defName>
    <include>
      <li>NamerWeapon</li>
    </include>
  </RulePackDef>


  <RulePackDef>
    <defName>NamerWeapon</defName>
    <include>
      <li>NamerArtUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>name->[personname]'s [badassconcept]</li>
        <li>name->[badassconcept]</li>
        <li>name->The [badassanimal]</li>
        <li>name->[badassadjective] [badassanimal]</li>
        <li>name->[badasscolor] [badassperson]</li>
        <li>name->[badasscolor] [badassanimal]</li>
        <li>name->[badassconcept] [badasscolor]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>badassconcept</keyword>
          <path>NameBanks/Nouns_Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassanimal</keyword>
          <path>NameBanks/Nouns_BadassAnimals</path>
        </li>
		        <li Class="Rule_File">
          <keyword>badassanimal_RP</keyword>
          <path>NameBanks/Nouns_BadassAnimals_RP</path>
        </li>
		       
		        <li Class="Rule_File">
          <keyword>badassanimal_plural</keyword>
          <path>NameBanks/Nouns_BadassAnimals_plural</path>
        </li>
				        <li Class="Rule_File">
          <keyword>badassanimal_many</keyword>
          <path>NameBanks/Nouns_BadassAnimals_many</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassadjective</keyword>
          <path>NameBanks/Adjectives_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassperson</keyword>
          <path>NameBanks/Nouns_BadassPersons</path>
        </li>
		<li Class="Rule_File">
          <keyword>badassperson_RP</keyword>
          <path>NameBanks/Nouns_BadassPersons_RP</path>
        </li>
				<li Class="Rule_File">
          <keyword>badassperson_plural</keyword>
          <path>NameBanks/Nouns_BadassPersons_plural</path>
        </li>
						<li Class="Rule_File">
          <keyword>badassperson_plural_RP</keyword>
          <path>NameBanks/Nouns_BadassPersons_plural_RP</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassconcept_RP</keyword>
          <path>NameBanks/Nouns_Concepts_Badass_RP</path>
        </li>
        <li Class="Rule_File">
          <keyword>badasscolor</keyword>
          <path>NameBanks/ColorsBadass</path>
        </li>
		<li Class="Rule_File">
          <keyword>badasscolor_RP</keyword>
          <path>NameBanks/ColorsBadass_RP</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>




  <RulePackDef>
    <defName>NamerSculpture</defName>
    <include>
      <li>NamerArtUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>name->[artname]</li>
        <li>name->[personname]'s [artname]</li>
        <li>name->[artname] [trans] [personname]</li>
        <li>name->[artname] [trans] [concept]</li>
        <li>name->[concept] [trans] [personname]</li>
        <li>name->[concept]</li>
        <li>name->[color] [concept]</li>
        <li>name->[color] [artname]</li>
        <li>name->[personname] [trans] [color]</li>
        <li>name->[color] [terrainfeature]</li>
        <li>name->A [art_adjective] [concept]</li>
        <li>name->[artname] [romannumeral]</li>
        <li>name->[concept] No. [digit]</li>
        <li>name->[artname] No. [digit]</li>
        <li>name->[artname] No. [digit][digit]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>



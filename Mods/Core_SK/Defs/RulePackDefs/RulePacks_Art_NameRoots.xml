<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>ArtName_Weapon</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>art_name->[badassconcept]</li>
        <li>art_name->The [badassanimal]</li>
        <li>art_name->[badassadjective] [badassanimal]</li>
        <li>art_name->[badasscolor] [badassanimal]</li>
        <li>art_name->[badassconcept] [badasscolor]</li>
        <li>art_name->[anyPawn_nameShort]'s [badassconcept]</li>
        <li>art_name->[badassconcept] of [anyPawn_nameShort]</li>
        <li>art_name->[badasscolor] [anyPawn_nameShort]</li>
        <li>art_name->[anyPawn_nameShort]'s [badassanimal]</li>
        <li>art_name->[anyPawn_nameShort]'s [badassperson]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>badassconcept</keyword>
          <path>NameBanks/Nouns_Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassanimal</keyword>
          <path>NameBanks/Nouns_BadassAnimals</path>
        </li>
		        <li Class="Rule_File">
          <keyword>badassanimal_RP</keyword>
          <path>NameBanks/Nouns_BadassAnimals_RP</path>
        </li>
		       
		        <li Class="Rule_File">
          <keyword>badassanimal_plural</keyword>
          <path>NameBanks/Nouns_BadassAnimals_plural</path>
        </li>
				        <li Class="Rule_File">
          <keyword>badassanimal_many</keyword>
          <path>NameBanks/Nouns_BadassAnimals_many</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassadjective</keyword>
          <path>NameBanks/Adjectives_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassperson</keyword>
          <path>NameBanks/Nouns_BadassPersons</path>
        </li>
		<li Class="Rule_File">
          <keyword>badassperson_RP</keyword>
          <path>NameBanks/Nouns_BadassPersons_RP</path>
        </li>
				<li Class="Rule_File">
          <keyword>badassperson_plural</keyword>
          <path>NameBanks/Nouns_BadassPersons_plural</path>
        </li>
						<li Class="Rule_File">
          <keyword>badassperson_plural_RP</keyword>
          <path>NameBanks/Nouns_BadassPersons_plural_RP</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassconcept_RP</keyword>
          <path>NameBanks/Nouns_Concepts_Badass_RP</path>
        </li>
        <li Class="Rule_File">
          <keyword>badasscolor</keyword>
          <path>NameBanks/ColorsBadass</path>
        </li>
		<li Class="Rule_File">
          <keyword>badasscolor_RP</keyword>
          <path>NameBanks/ColorsBadass_RP</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>ArtName_WeaponMelee</defName>
    <include>
      <li>ArtName_Weapon</li>
    </include>
  </RulePackDef>

  <RulePackDef>
    <defName>ArtName_WeaponGun</defName>
    <include>
      <li>ArtName_Weapon</li>
    </include>
  </RulePackDef>


  <RulePackDef>
    <defName>ArtName_Sculpture</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>art_name->[anyPawn_nameShort]'s [artname]</li>
        <li>art_name->[anyPawn_nameShort] [trans] [color]</li>
        <li>art_name->[artname] [trans] [anyPawn_nameShort]</li>
        <li>art_name->[artname] [trans] [concept]</li>
        <li>art_name->[artname] [romannumeral]</li>
        <li>art_name->[concept] [trans] [anyPawn_nameShort]</li>
        <li>art_name->[concept] [trans] [anyPawn_nameShort]</li>
        <li>art_name->[concept]</li>
        <li>art_name->[color] [concept]</li>
        <li>art_name->[color] [artname]</li>
        <li>art_name->[color] [terrainfeature]</li>
        <li>art_name->A [art_adjective] [concept]</li>
        <li>art_name->[concept] No. [digit]</li>
        <li>art_name->[artname] No. [digit]</li>
        <li>art_name->[artname] No. [digit][digit]</li>
        <li>art_name->[digit] [trans] [anyPawn_nameShort]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>


  <RulePackDef>
    <defName>ArtName_Furniture</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>art_name->[anyPawn_nameShort]'s [positiveconcept]</li>
        <li>art_name->[positiveconcept]</li>
        <li>art_name->The [animal]</li>
        <li>art_name->[badasscolor] [animal]</li>
        <li>art_name->[anyPawn_nameShort]'s [color]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>positiveconcept</keyword>
          <path>NameBanks/Nouns_Concepts_Positive</path>
        </li>
		<li Class="Rule_File">
          <keyword>positiveconcept_RP</keyword>
          <path>NameBanks/Nouns_Concepts_Positive_RP</path>
        </li>
        <li Class="Rule_File">
          <keyword>animal</keyword>
          <path>NameBanks/Nouns_Animals</path>
        </li>
		<li Class="Rule_File">
          <keyword>animal_plural</keyword>
          <path>NameBanks/Nouns_Animals_plural</path>
        </li>
		<li Class="Rule_File">
          <keyword>animal_plural_RP</keyword>
          <path>NameBanks/Nouns_Animals_plural_RP</path>
        </li>
				<li Class="Rule_File">
          <keyword>animal_plural_TP</keyword>
          <path>NameBanks/Nouns_Animals_plural_TP</path>
        </li>
				<li Class="Rule_File">
          <keyword>animal_rp</keyword>
          <path>NameBanks/Nouns_Animals_RP</path>
        </li>
        <li Class="Rule_File">
          <keyword>badasscolor</keyword>
          <path>NameBanks/ColorsBadass</path>
        </li>
		<li Class="Rule_File">
          <keyword>badasscolor_RP</keyword>
          <path>NameBanks/ColorsBadass_RP</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
  
  <RulePackDef>
    <defName>ArtName_SarcophagusPlate</defName>
    <include>
      <li>ArtNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>art_name->[angstyadjective] [anyPawn_nameShort]</li>
        <li>art_name->[anyPawn_nameShort]</li>
        <li>art_name->[color] [anyPawn_nameShort]</li>
        <li>art_name->[angstyadjective] [angstyconcept]</li>
        <li>art_name->[angstyadjective] [badassconcept]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>angstyadjective</keyword>
          <path>NameBanks/Adjectives_Angsty</path>
        </li>
		<li Class="Rule_File">
          <keyword>angstyadjective_RP</keyword>
          <path>NameBanks/Adjectives_Angsty_RP</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassperson</keyword>
          <path>NameBanks/Nouns_BadassPersons</path>
        </li>
		<li Class="Rule_File">
          <keyword>badassperson_RP</keyword>
          <path>NameBanks/Nouns_BadassPersons_RP</path>
        </li>
								<li Class="Rule_File">
          <keyword>badassperson_plural_RP</keyword>
          <path>NameBanks/Nouns_BadassPersons_plural_RP</path>
        </li>
						<li Class="Rule_File">
          <keyword>badassperson_plural</keyword>
          <path>NameBanks/Nouns_BadassPersons_plural</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>NameBanks/Colors</path>
        </li>
		<li Class="Rule_File">
          <keyword>color_RP</keyword>
          <path>NameBanks/Colors_RP</path>
        </li>
        <li Class="Rule_File">
          <keyword>angstyconcept</keyword>
          <path>NameBanks/Nouns_Concepts_Angsty</path>
        </li>
		<li Class="Rule_File">
          <keyword>angstyconcept_RP</keyword>
          <path>NameBanks/Nouns_Concepts_Angsty_RP</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassconcept</keyword>
          <path>NameBanks/Nouns_Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>badassconcept_RP</keyword>
          <path>NameBanks/Nouns_Concepts_Badass_RP</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>




  <RulePackDef>
    <defName>ArtNameUtility</defName>
    <rulePack>
      <rulesStrings>
        <li>trans->of</li>
        <li>trans->for</li>
        <li>trans->by</li>
        <li>trans->with</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>art_adjective</keyword>
          <pathList>
            <li>NameBanks/Adjectives_Badass</li>
            <li>NameBanks/Adjectives_Angsty</li>
            <li>NameBanks/Adjectives_Angsty</li>
          </pathList>
        </li>
		<li Class="Rule_File">
          <keyword>art_adjective_RP</keyword>
          <pathList>
            <li>NameBanks/Adjectives_Badass_RP</li>
            <li>NameBanks/Adjectives_Angsty_RP</li>
            <li>NameBanks/Adjectives_Angsty_RP</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>NameBanks/Nouns_TerrainFeatures</path>
        </li>
		
		        <li Class="Rule_File">
          <keyword>terrainfeature_peredza</keyword>
          <path>NameBanks/Nouns_TerrainFeatures_PEREDZA</path>
        </li>
						        <li Class="Rule_File">
          <keyword>terrainfeature_color</keyword>
          <path>NameBanks/Nouns_TerrainFeatures_color</path>
        </li>
        <li Class="Rule_PersonName">
          <selectionWeight>25</selectionWeight>
          <keyword>personname</keyword>
        </li>
        <li Class="Rule_File">
          <keyword>artname</keyword>
          <path>NameBanks/Nouns_Artworks</path>
        </li>
		<li Class="Rule_File">
          <keyword>artname_RP</keyword>
          <path>NameBanks/Nouns_Artworks_RP</path>
        </li>
        <li Class="Rule_File">
          <keyword>concept</keyword>
          <pathList>
            <li>NameBanks/Nouns_Concepts_Badass</li>
            <li>NameBanks/Nouns_Concepts_Positive</li>
            <li>NameBanks/Nouns_Concepts_Angsty</li>
            <li>NameBanks/Nouns_Concepts_Angsty</li>
          </pathList>
        </li>
		<li Class="Rule_File">
          <keyword>concept_RP</keyword>
          <pathList>
            <li>NameBanks/Nouns_Concepts_Badass_RP</li>
            <li>NameBanks/Nouns_Concepts_Positive_RP</li>
            <li>NameBanks/Nouns_Concepts_Angsty_RP</li>
            <li>NameBanks/Nouns_Concepts_Angsty_RP</li>
          </pathList>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>


</Defs>
<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- Orassan Bases -->
    <PawnKindDef Name="OrassanVillagerBase" Abstract="True">
        <race>Alien_Orassan</race>
        <baseRecruitDifficulty>0.70</baseRecruitDifficulty>
        <backstoryCategory>Orassan</backstoryCategory>
        <defaultFactionType>Orassan</defaultFactionType>
        <chemicalAddictionChance>0.1</chemicalAddictionChance>
        <invNutrition>2</invNutrition>
        <invFoodDef>MealJerky</invFoodDef>
        <itemQuality>Normal</itemQuality>
        <backstoryCryptosleepCommonality>0.25</backstoryCryptosleepCommonality>
        <maxGenerationAge>65</maxGenerationAge>
        <gearHealthRange>
            <min>0.5</min>
            <max>1.5</max>
        </gearHealthRange>
        <techHediffsMoney>
            <min>600</min>
            <max>1200</max>
        </techHediffsMoney>
        <techHediffsTags>
            <li>Advanced</li>
        </techHediffsTags>
        <techHediffsChance>0.006</techHediffsChance>
    </PawnKindDef>
	
	<!-- Refugee -->
    <PawnKindDef ParentName="OrassanVillagerBase">
        <defName>OrassanRefugee</defName>
        <label>orassan refugee</label>
        <combatPower>70</combatPower>
        <isFighter>false</isFighter>
        <apparelMoney>
            <min>1200</min>
            <max>1750</max>
        </apparelMoney>
        <apparelTags>
            <li>PredatorsLight</li>
            <li>BanditsLight</li>
			<li>Outlander</li>
        </apparelTags>
        <apparelAllowHeadwearChance>0.4</apparelAllowHeadwearChance>
    <inventoryOptions>
      <skipChance>0.75</skipChance>
      <subOptionsChooseOne>
        <li>
          <choiceChance>80</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>50</min>
            <max>200</max>
          </countRange>
        </li>
        <li>
          <choiceChance>20</choiceChance>
          <thingDef>Silver</thingDef>
          <countRange>
            <min>250</min>
            <max>900</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
    </PawnKindDef>
	
	<!-- Villager -->
    <PawnKindDef Name="OrassanVillager" ParentName="OrassanVillagerBase">
        <defName>OrassanVillager</defName>
        <label>orassan villager</label>
        <combatPower>70</combatPower>
        <isFighter>false</isFighter>
        <apparelMoney>
            <min>1200</min>
            <max>1750</max>
        </apparelMoney>
        <apparelTags>
            <li>PredatorsLight</li>
            <li>BanditsLight</li>
        </apparelTags>
        <apparelAllowHeadwearChance>0.4</apparelAllowHeadwearChance>
        <weaponMoney>
            <min>1500</min>
            <max>2550</max>
        </weaponMoney>
        <weaponTags>
            <li>SMG2</li>
        </weaponTags>
        <modExtensions>
            <li Class="CombatExtended.LoadoutPropertiesExtension">
                <primaryMagazineCount>
                    <min>9</min>
                    <max>10</max>
                </primaryMagazineCount>
            </li>
        </modExtensions>
    </PawnKindDef>

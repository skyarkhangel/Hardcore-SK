<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="CombatExtended.PatchOperationFindMod">
				<modName>A Dog Said...</modName>
			</li>

			<li Class="PatchOperationAdd"> 
				<xpath>/RecipeDefs/RecipeDef[@Name = "ProstheticAnimalCraftBase"]/RecipeUsers</xpath>
				<value>
					<li>TablePrimitiveProsthetic</li>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationSequence"> <!--MakeProstheticTailAnimal-->
		<success>Always</success>
		<operations>
			<li Class="CombatExtended.PatchOperationFindMod">
				<modName>A Dog Said...</modName>
			</li>

			<li Class="PatchOperationReplace"> 
				<xpath>/RecipeDefs/RecipeDef[defName="MakeProstheticTailAnimal"]/ingredients</xpath>
				<value>
					<ingredients>
						<li>
							<filter>
								<categories>
									<li>Metallic</li>
								</categories>
							</filter>
							<count>25</count>
						</li>
						<li>
							<filter>
								<thingDefs>
									<li>Mechanism</li>
								</thingDefs>
							</filter>
							<count>1</count>
						</li>
					</ingredients>
				</value>
			</li>					
			<li Class="PatchOperationReplace"> 
				<xpath>/RecipeDefs/RecipeDef[defName="MakeProstheticTailAnimal"]/fixedIngredientFilter</xpath>
				<value>
					<fixedIngredientFilter>
						<categories>
							<li>SLDBar</li>
							<li>USLDBar</li>
						</categories>
						<thingDefs>
							<li>Mechanism</li>
						</thingDefs>
					</fixedIngredientFilter>
				</value>
			</li>		
		</operations>
	</Operation>


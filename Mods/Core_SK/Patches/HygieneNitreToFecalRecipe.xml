<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dubs Bad Hygiene</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs</xpath>
			<value>
				<RecipeDef Class="UniversalFermenter.RecipeDef_UF">
					<defName>Make_Nitre_Fecal</defName>
					<label>Extracts nitre from fecal</label>
					<description>Extract nitre from fecal sludge. Takes 10 days to make</description>
					<ingredients>
						<li>
							<filter>
								<thingDefs>
									<li>FecalSludge</li>
								</thingDefs>
							</filter>
							<count>350</count>
						</li>
						<li>
							<filter>
								<thingDefs>
									<li>Ash</li>
								</thingDefs>
							</filter>
							<count>1</count>
						</li>
					</ingredients>
					<products>
						<Nitre>15</Nitre>
					</products>
					<recipeUsers>
						<li>Saltpeter_pit</li>
					</recipeUsers>
					
					<!-- RecipeDef_UF fields -->
					<temperatureSafe>
						<min>5</min>
						<max>35</max>
					</temperatureSafe>
					<temperatureIdeal>
						<min>10</min>
						<max>30</max>
					</temperatureIdeal>
					<ruinedPerDegreePerHour>0</ruinedPerDegreePerHour>
					<processDays>10</processDays>
					<speedBelowSafe>0.01</speedBelowSafe>
					<speedAboveSafe>0.8</speedAboveSafe>
					<graphicSuffix>_closed</graphicSuffix>
				</RecipeDef>
			</value>
		</match>
	</Operation>
</Patch>
<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <Operation Class="PatchOperationFindMod">
    <mods>
        <li>Misc. Training</li>
    </mods>
    <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="MartialArtsTarget"]</xpath>
                    <value>
                        <researchPrerequisites>
                            <li>Smithing</li>
                        </researchPrerequisites>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="ShootingRangeTarget"]</xpath>
                    <value>
                        <researchPrerequisites>
                            <li>Greatbow</li>
                        </researchPrerequisites>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="ArcheryTarget"]</xpath>
                    <value>
                        <researchPrerequisites>
                            <li>RecurveBow</li>
                        </researchPrerequisites>
                    </value>
                </li>
            </operations>
    </match>
    </Operation>

</Patch>
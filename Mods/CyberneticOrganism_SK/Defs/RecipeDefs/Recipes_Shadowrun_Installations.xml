      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>ANN_EnergyCore</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Heart</li>      <!-- Torso -->
    </appliedOnFixedBodyParts>
    <addsHediff>Trunken_hediff_EnergyCore</addsHediff>
    <descriptionHyperlinks><ThingDef>ANN_EnergyCore</ThingDef></descriptionHyperlinks>
    <researchPrerequisite>Prosthesis_install_E2</researchPrerequisite>
  </RecipeDef>


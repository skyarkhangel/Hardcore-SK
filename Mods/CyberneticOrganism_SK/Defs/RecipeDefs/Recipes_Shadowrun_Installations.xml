<Defs>

<!-- Energy Core -->
  <RecipeDef ParentName="SK_SurgeryFlesh">
    <defName>ANN_InstallEnergyCore</defName>
    <label>install energy core</label>
    <description>Install an energy Core.</description>
    <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
    <jobString>Installing heart.</jobString>
    <workAmount>5000</workAmount>
    <deathOnFailedSurgeryChance>0.15</deathOnFailedSurgeryChance>
    <surgerySuccessChanceFactor>0.90</surgerySuccessChanceFactor>
    <recipeUsers>
      <li>Human</li>
    </recipeUsers>
    <skillRequirements>
      <Medicine>16</Medicine>
    </skillRequirements>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>3</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ANN_EnergyCore</li>

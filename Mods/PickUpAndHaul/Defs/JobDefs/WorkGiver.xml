    <reportString>gathering TargetA to hauling inventory.</reportString>
	<casualInterruptible>false</casualInterruptible>
    <suspendable>false</suspendable>
  </JobDef>
  
  <JobDef>
    <defName>UnloadYourHauledInventory</defName>
    <driverClass>PickUpAndHaul.JobDriver_UnloadYourHauledInventory</driverClass>
    <reportString>unloading inventory.</reportString>
	<casualInterruptible>false</casualInterruptible>
    <suspendable>false</suspendable>
  </JobDef>
  

	

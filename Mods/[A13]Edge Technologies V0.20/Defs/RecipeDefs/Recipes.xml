<Defs>
  <!--Robotics-->
  <RecipeDef>
    <defName>MakeRobotics</defName>
    <label>Make Robotics</label>
    <description>Make Robotics.</description>
    <jobString>Making robotics.</jobString>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>2500</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Components</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>Plasteel</li>
        <li>Components</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Robotics>10</Robotics>
    </products>
    <skillRequirements>
      <li>
        <skill>Crafting</skill>
        <minLevel>7</minLevel>
      </li>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

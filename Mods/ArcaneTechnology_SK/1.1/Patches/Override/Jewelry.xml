<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="DArcaneTechnology.OverrideResearch">
	<Overrides>
		<!-- Override will take effect even if an item is otherwise associated with a research project; it'll override the association -->
		<li>
			<thingDef>Jewelry_PsychicNecklace</thingDef>
			<researchDefName>Jewelry</researchDefName>
		</li>
		<!-- None does what it sounds like - no research project will be associated. Club included just for example. -->
		<li>
			<thingDef>Club</thingDef>
			<researchDefName>None</researchDefName>
		</li>
	</Overrides>
</Operation>

</Patch>
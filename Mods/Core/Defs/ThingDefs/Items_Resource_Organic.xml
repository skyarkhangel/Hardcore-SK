      <MarketValue>7.0</MarketValue>
    </statBases>
    <comps>
      <li>
        <compClass>CompHatcher</compClass>
        <hatcherDaysToHatch>7</hatcherDaysToHatch>
        <hatcherPawn>Chicken</hatcherPawn>
      </li>
    </comps>
  </ThingDef>


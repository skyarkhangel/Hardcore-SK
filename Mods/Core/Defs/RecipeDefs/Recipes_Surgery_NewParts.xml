    <label>install heart</label>
    <description>Installs heart.</description>
    <workerClass>Recipe_InstallNaturalBodyPart</workerClass>
    <jobString>Installing heart.</jobString>
    <workAmount>2000</workAmount>
    <ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>

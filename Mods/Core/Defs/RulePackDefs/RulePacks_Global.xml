<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>GlobalUtility</defName>
    <rulePack>
      <rulesStrings>
        <li>quantity_adjphrase->a group of</li>
        <li>quantity_adjphrase->several</li>
        <li>quantity_adjphrase->dozens of</li>
        <li>quantity_adjphrase->two</li>
        <li>quantity_adjphrase->three</li>
        <li>quantity_adjphrase->four</li>
        <li>quantity_adjphrase->five</li>
        <li>quantity_adjphrase->six</li>
        <li>quantity_adjphrase->uncountable</li>
        <li>quantity_adjphrase->innumerable</li>
        <li>trans->of</li>
        <li>trans->for</li>
        <li>trans->by</li>
        <li>trans->with</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_Number">
          <selectionWeight>10</selectionWeight>
          <keyword>digit</keyword>
          <range>
            <min>0</min>
            <max>9</max>
          </range>
        </li>
        <li Class="Rule_File">
          <keyword>vegetable</keyword>
          <path>Words/Nouns/Vegetables</path>
        </li>
        <li Class="Rule_File">
          <keyword>game</keyword>
          <path>Words/Nouns/Games</path>
        </li>
        <li Class="Rule_File">
          <keyword>letter</keyword>
          <path>WordParts/CapitalLetters</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
        <li Class="Rule_File">
          <keyword>animal</keyword>
          <path>Words/Nouns/Animals</path>
        </li>
        <li Class="Rule_File">
          <keyword>animalgroup</keyword>
          <path>Words/Nouns/AnimalGroups</path>
        </li>
        <li Class="Rule_File">
          <keyword>weapon</keyword>
          <path>Words/Nouns/Weapons</path>
        </li>
        <li Class="Rule_File">
          <keyword>romannumeral</keyword>
          <path>Words/Misc/Numbers_SmallRoman</path>
        </li>
        <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
        <li Class="Rule_File">
          <keyword>naturalobject</keyword>
          <path>Words/Nouns/NaturalObject</path>
        </li>
        <li Class="Rule_File">
          <keyword>tree</keyword>
          <path>Words/Nouns/Trees</path>
        </li>
        <li Class="Rule_File">
          <keyword>outlander</keyword>
          <pathList>
            <li>Words/Nouns/People_Outlanders</li>
            <li>Words/Nouns/People_Family</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>enemy</keyword>
          <pathList>
            <li>Words/Nouns/People_Enemies</li>
            <li>Words/Nouns/People_Badass</li>
          </pathList>
        </li>
        <li Class="Rule_PersonName">
          <selectionWeight>25</selectionWeight>
          <keyword>personname</keyword>
        </li>
        <li Class="Rule_File">
          <keyword>apparel</keyword>
          <path>Words/Nouns/Apparel</path>
        </li>
        <li Class="Rule_File">
          <keyword>large</keyword>
          <pathList>
            <li>Words/Adjectives/Large</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>naturaladjective</keyword>
          <pathList>
            <li>Words/Adjectives/Natural</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>adjective</keyword>
          <pathList>
            <li>Words/Adjectives/Badass</li>
            <li>Words/Adjectives/Angsty</li>
            <li>Words/Adjectives/Friendly</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>badassadjective</keyword>
          <path>Words/Adjectives/Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>angstyadjective</keyword>

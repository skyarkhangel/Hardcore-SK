<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>GlobalUtility</defName>
    <rulePack>
      <rulesStrings>
        <li>quantity_adjphrase->a group of</li>
        <li>quantity_adjphrase->several</li>
        <li>quantity_adjphrase->dozens of</li>
        <li>quantity_adjphrase->two</li>
        <li>quantity_adjphrase->three</li>
        <li>quantity_adjphrase->four</li>
        <li>quantity_adjphrase->five</li>
        <li>quantity_adjphrase->six</li>
        <li>quantity_adjphrase->uncountable</li>
        <li>quantity_adjphrase->innumerable</li>
        <li>game->poker</li>
        <li>game->chess</li>
        <li>game->cards</li>
        <li>game->backgammon</li>
        <li>game->dice</li>
        <li>game->craps</li>
        <li>game->roulette</li>
        <li>game->blackjack</li>
        <li>vegetable->tomato</li>
        <li>vegetable->carrot</li>
        <li>vegetable->onion</li>
        <li>vegetable->turnip</li>
        <li>vegetable->bok choy</li>
        <li>vegetable->stalk of celery</li>
        <li>vegetable->gourd</li>
        <li>vegetable->pumpkin</li>
        <li>vegetable->broccoli</li>
        <li>vegetable->cauliflower</li>
        <li>vegetable->bell pepper</li>
        <li>vegetable->potato</li>
        <li>vegetable->mushroom</li>
        <li>trans->of</li>
        <li>trans->for</li>
        <li>trans->by</li>
        <li>trans->with</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_Number">
          <selectionWeight>10</selectionWeight>
          <keyword>digit</keyword>
          <range>
            <min>0</min>
            <max>9</max>
          </range>
        </li>
        <li Class="Rule_File">
          <keyword>letter</keyword>
          <path>NameBanks/CapitalLetters</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>NameBanks/Colors</path>
        </li>
        <li Class="Rule_File">
          <keyword>animal</keyword>
          <path>NameBanks/Nouns_Animals</path>
        </li>
        <li Class="Rule_File">
          <keyword>weapon</keyword>
          <path>NameBanks/Nouns_Weapons</path>
        </li>
        <li Class="Rule_File">
          <keyword>romannumeral</keyword>
          <path>NameBanks/Numbers_SmallRoman</path>
        </li>
        <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>NameBanks/Nouns_TerrainFeatures</path>
        </li>
        <li Class="Rule_File">
          <keyword>outlander</keyword>
          <pathList>
            <li>NameBanks/Nouns_People_Outlanders</li>
            <li>NameBanks/Nouns_People_Family</li>
          </pathList>
        </li>
        <li Class="Rule_PersonName">
          <selectionWeight>25</selectionWeight>
          <keyword>personname</keyword>
        </li>
        <li Class="Rule_File">
          <keyword>apparel</keyword>
          <path>NameBanks/Nouns_Apparel</path>
        </li>
        <li Class="Rule_File">
          <keyword>adjective</keyword>
          <pathList>
            <li>NameBanks/Adjectives_Badass</li>
            <li>NameBanks/Adjectives_Angsty</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>concept</keyword>
          <pathList>
            <li>NameBanks/Nouns_Concepts_Badass</li>
            <li>NameBanks/Nouns_Concepts_Positive</li>
            <li>NameBanks/Nouns_Concepts_Angsty</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>community</keyword>
          <path>NameBanks/Nouns_Communities</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
  
</Defs>
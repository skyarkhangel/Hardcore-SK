        </li>
        <li Class="Rule_File">
          <keyword>concept</keyword>
          <pathList>
            <li>Words/Nouns/Concepts_Badass</li>
            <li>Words/Nouns/Concepts_Positive</li>
            <li>Words/Nouns/Concepts_Angsty</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>community</keyword>
          <path>Words/Nouns/Communities</path>
        </li>
        <li Class="Rule_File">
          <keyword>bodypart</keyword>
          <pathList>
            <li>Words/Nouns/Bodyparts</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>direction</keyword>
          <path>Words/Adjectives/CompassDirections</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
  
</Defs>

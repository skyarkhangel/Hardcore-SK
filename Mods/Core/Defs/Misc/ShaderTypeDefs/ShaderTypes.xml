<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--
    Cutout:
      The default material for cutout world objects
	    Will draw in correct Z order with respect to each other
  -->
  <ShaderTypeDef>
    <defName>Cutout</defName>
    <shaderPath>Map/Cutout</shaderPath>
  </ShaderTypeDef>

  <ShaderTypeDef>
    <defName>CutoutFlying</defName>
    <shaderPath>Map/CutoutFlying</shaderPath>
  </ShaderTypeDef>

  <ShaderTypeDef>
    <defName>CutoutPlant</defName>
    <shaderPath>Map/CutoutPlant</shaderPath>
  </ShaderTypeDef>

  <ShaderTypeDef>
    <defName>CutoutComplex</defName>
    <shaderPath>Map/CutoutComplex</shaderPath>
  </ShaderTypeDef>

  <ShaderTypeDef>
    <defName>CutoutSkin</defName>
    <shaderPath>Map/CutoutSkin</shaderPath>
  </ShaderTypeDef>

  <!--
    Transparent:
      Non-alpha affecting non-Z-writing version of default material
	    Will draw in correct Z order with respect to UnlitCutout (because it does a ZTest)
	    Will _not_ draw in consistent Z order with respect to each other (because it does not ZWrite).
  -->
  <ShaderTypeDef>
    <defName>Transparent</defName>
    <shaderPath>Map/Transparent</shaderPath>
  </ShaderTypeDef>

  <ShaderTypeDef>
    <defName>TransparentPostLight</defName>
    <shaderPath>Map/TransparentPostLight</shaderPath>
  </ShaderTypeDef>

  <ShaderTypeDef>
    <defName>TransparentPlant</defName>
    <shaderPath>Map/TransparentPlant</shaderPath>
  </ShaderTypeDef>

  <ShaderTypeDef>
    <defName>MetaOverlay</defName>
    <shaderPath>Map/MetaOverlay</shaderPath>
  </ShaderTypeDef>

  <ShaderTypeDef>
    <defName>Mote</defName>
    <shaderPath>Map/Mote</shaderPath>
  </ShaderTypeDef>

  <ShaderTypeDef>
    <defName>MoteGlow</defName>
    <shaderPath>Map/MoteGlow</shaderPath>
  </ShaderTypeDef>

  <ShaderTypeDef>
    <defName>MoteWater</defName>
    <shaderPath>Map/MoteWater</shaderPath>
  </ShaderTypeDef>
  
  <ShaderTypeDef>
    <defName>EdgeDetect</defName>
    <shaderPath>Map/EdgeDetect</shaderPath>
  </ShaderTypeDef>

</Defs>

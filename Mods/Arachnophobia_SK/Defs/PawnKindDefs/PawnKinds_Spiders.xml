  ==============================================================================
  -->

    <BodyDef>
      <defName>ROMA_SpiderBody</defName>
      <label>spider-like</label>
      <corePart>
        <def>Shell</def>
        <height>Middle</height>
        <depth>Outside</depth>
        <parts>
          <li>
            <def>Stomach</def>
            <coverage>0.05</coverage>
            <depth>Inside</depth>
          </li>
          <li>
            <def>InsectHeart</def>
            <coverage>0.03</coverage>
            <depth>Inside</depth>
          </li>
          <li>
            <def>ROMA_Cephalothorax</def>
            <coverage>0.18</coverage>
            <height>Top</height>
            <parts>
              <li>
                <def>InsectHead</def>
                <coverage>0.65</coverage>
                <groups>
                  <li>HeadAttackTool</li>
                </groups>
                <parts>
                  <li>
                    <def>Brain</def>
                    <coverage>0.2</coverage>
                    <depth>Inside</depth>
                  </li>
                  <li>
                    <def>LeftEye</def>
                    <coverage>0.15</coverage>
                  </li>
                  <li>
                    <def>RightEye</def>
                    <coverage>0.15</coverage>
                  </li>
                  <li>
                    <def>InsectNostril</def>
                    <coverage>0.07</coverage>
                  </li>
                  <li>
                    <def>InsectMouth</def>
                    <coverage>0.10</coverage>
                    <groups>
                      <li>Mouth</li>
                    </groups>
                  </li>
                </parts>
              </li>
            </parts>
          </li>
          <li>
            <def>ROMA_LeftPedipalp</def>
            <coverage>0.06</coverage>
            <height>Bottom</height>
          </li>
          <li>
            <def>ROMA_RightPedipalp</def>
            <coverage>0.06</coverage>
            <height>Bottom</height>
          </li>

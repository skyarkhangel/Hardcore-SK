	<success>Always</success>
	<operations>
		<li Class="ModCheck.isModLoaded">
			<modName>Extended Human Body Simulation v1.0.0</modName>
			<yourMod>Diseases overhauled v1.1.0</yourMod>
			<errorOnFail>true</errorOnFail>
			<customMessageFail>Require enabled Extended Human Body Simulation v1.0.0</customMessageFail>
		</li>
		<li Class="ModCheck.loadOrder">
			<modName>Extended Human Body Simulation v1.0.0</modName>
			<yourMod>Diseases overhauled v1.1.0</yourMod>
			<errorOnFail>true</errorOnFail>
			<customMessageFail>Should load after Extended Human Body Simulation v1.0.0</customMessageFail>
		</li>
	</operations>
  </Operation>
   
</Patch>
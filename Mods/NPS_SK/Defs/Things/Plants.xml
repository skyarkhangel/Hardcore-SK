<?xml version="1.0" encoding="UTF-8"?>
<Defs>

   <ThingDef ParentName="TreeBase">
      <defName>TKKN_OldGrowth_PlantTreeHazelnut</defName>
      <label>old growth hazelnut tree</label>
      <description>A multi-stemmed woodland shrub with soft, saw-toothed, velvety leaves.</description>
      <graphicData>
         <texPath>TKKN_NPS/Things/Plant/HazelOldGrowth</texPath>
         <graphicClass>Graphic_Random</graphicClass>
      </graphicData>
      <statBases>
         <Beauty>2</Beauty>
      </statBases>
      <plant>
         <sowTags />
         <growDays>50</growDays>
         <harvestWork>1000</harvestWork>
         <harvestYield>45</harvestYield>
         <wildClusterSizeRange>
            <min>2</min>
            <max>4</max>
         </wildClusterSizeRange>
         <wildClusterRadius>5</wildClusterRadius>
         <visualSizeRange>
            <min>1.5</min>
            <max>3.5</max>
         </visualSizeRange>
      </plant>
   </ThingDef>

    <label>extract mint seeds</label>
    <description>Extract seeds from mint plants.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>MintLeaves</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>MintLeaves</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <SeedMint>3</SeedMint>
    </products>
    <researchPrerequisite>Medical_Plants_A3</researchPrerequisite>
  </RecipeDef>
  

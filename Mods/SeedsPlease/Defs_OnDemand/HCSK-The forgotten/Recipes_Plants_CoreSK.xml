<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedblueberry</defName>
    <label>extract blueberry seeds</label>
    <description>Extract seeds from blueberry.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Rawblueberry</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Rawblueberry</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seedblueberry>3</Seedblueberry>
    </products>
    <researchPrerequisite>SK_FruitsII</researchPrerequisite>
  </RecipeDef>
  

<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--Mediflower-->
	<ThingDef ParentName="CropsBase">
		<defName>WildRose</defName>
		<label>Wild Rose</label>
		<description>Wild Roses are the red flowers of a Wild Rose Plant. The plant is also known as the Prickly Rose, Bristly Rose and Arctic Rose.</description>
		<graphicData>
			<texPath>Things/Item/Resource/mediflower</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<altitudeLayer>Item</altitudeLayer>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Beauty>5</Beauty>
			<MarketValue>2</MarketValue>
			<DeteriorationRate>5</DeteriorationRate>
			<Flammability>5</Flammability>
			<Nutrition>0.01</Nutrition>
		</statBases>
		<socialPropernessMatters>true</socialPropernessMatters>
		<alwaysHaulable>true</alwaysHaulable>
		<drawGUIOverlay>true</drawGUIOverlay>
		<rotatable>false</rotatable>
		<pathCost>15</pathCost>
		<soundInteract>Standard_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<useHitPoints>true</useHitPoints>
		<ingestible>
			<foodType>Plant, VegetableOrFruit</foodType>
			<preferability>RawBad</preferability>
			<joy>0.04</joy>
			<joyKind>Gluttonous</joyKind>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>RawVegetable_Eat</ingestSound>
		</ingestible>
		<comps>
			<li Class="CompProperties_Forbiddable"/>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>7</daysToRotStart>
			</li>
		</comps>
		<tickerType>Rare</tickerType>   
		<thingCategories>
			<li>LuxuryStuffs</li>
		</thingCategories>
		<stackLimit>75</stackLimit>
	</ThingDef>


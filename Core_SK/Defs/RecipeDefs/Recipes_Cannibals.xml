<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<RecipeDef>
		<defName>MinceOrgans</defName>
		<label>mince organs</label>
		<description>Minces human organs to obtain edible meat. Makes great prison meals.</description>
		<jobString>Butchering.</jobString>
		<workAmount>250</workAmount>
		<recipeUsers>
			  <li>TableButcher</li>
		</recipeUsers>
		<workSpeedStat>ButcheryFleshSpeed</workSpeedStat>
		<workSkill>Cooking</workSkill>
		<effectWorking>ButcherFlesh</effectWorking>
		<soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>BodyPartsNatural</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>BodyPartsNatural</li>
			</categories>
			<disallowedThingDefs>
				<li>Leg</li>
				<li>Arm</li>
				<li>Ear</li>
				<li>Eye</li>
				<li>Nose</li>
				<li>Jaw</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<products>
			<Meat_Human>25</Meat_Human>
		</products>
		<researchPrerequisite>Food_0</researchPrerequisite>
	</RecipeDef>

	<RecipeDef>
		<defName>MinceLimbs</defName>
		<label>mince limbs</label>
		<description>Minces human limbs to obtain edible meat. Watch out for toenails.</description>
		<jobString>Butchering.</jobString>
		<workAmount>300</workAmount>
		<recipeUsers>
			  <li>TableButcher</li>
		</recipeUsers>
		<workSpeedStat>ButcheryFleshSpeed</workSpeedStat>
		<workSkill>Cooking</workSkill>
		<effectWorking>ButcherFlesh</effectWorking>
		<soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>BodyPartsNatural</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>BodyPartsNatural</li>
			</categories>
			<disallowedThingDefs>
				<li>Stomach</li>
				<li>Liver</li>
				<li>Ear</li>
				<li>Eye</li>
				<li>Nose</li>
				<li>Kidney</li>
				<li>Jaw</li>
				<li>Heart</li>
				<li>Lung</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<products>
			<Meat_Human>50</Meat_Human>
		</products>
		<researchPrerequisite>Food_0</researchPrerequisite>
	</RecipeDef>

</Defs>
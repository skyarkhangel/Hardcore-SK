		<soundWorking>Recipe_Smelt</soundWorking>
		<recipeUsers>
			<li>RobAssem</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Biomatter</li>
					</thingDefs>
				</filter>
				<count>15</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SyntheticFibers</li>
					</thingDefs>
				</filter>

<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>BuildMagneticMaterial</defName>
		<label>Synthesize magnetic material</label>
		<description>Synthesize strong magnetic material from magnetic metals. Produces 5.</description>
		<jobString>Synthesizing magnetic material.</jobString>
		<workAmount>2400</workAmount>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Smelt</soundWorking>
		<recipeUsers>
			<li>RobAssem</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Matter</li>
					</thingDefs>
				</filter>
				<count>30</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Sulphates</li>
					</thingDefs>
				</filter>
				<count>15</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>USLDBar</li>
					</categories>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<categories>
						<li>SLDBar</li>
					</categories>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<products>
			<MagneticMaterial>5</MagneticMaterial>
		</products>
		<fixedIngredientFilter>
			<categories>
				<li>SLDBar</li>
				<li>USLDBar</li>
			</categories>
			<thingDefs>
				<li>Matter</li>
				<li>Sulphates</li>
			</thingDefs>
		</fixedIngredientFilter>
		<skillRequirements>
			<Crafting>18</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.8</workSkillLearnFactor>
        <researchPrerequisite>Components_D3</researchPrerequisite>
	</RecipeDef>


	<RecipeDef>

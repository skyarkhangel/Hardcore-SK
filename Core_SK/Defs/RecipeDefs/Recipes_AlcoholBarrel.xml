<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef Name="RecipeCrateBase" Abstract="True">
		<description>Take all of the alcohol bottles out of their storage crate.</description>
		<jobString>Emptying crate.</jobString>
		<workSpeedStat>DrugCookingSpeed</workSpeedStat>
		<workAmount>50</workAmount>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_Brewing</soundWorking>
		<recipeUsers>
			<li>Brewery</li>
			<li>ElectricBrewery</li>
		</recipeUsers>
		<workSkill>Cooking</workSkill>
		<researchPrerequisite>Brewing</researchPrerequisite>
	</RecipeDef>


	<RecipeDef ParentName="RecipeCrateBase">
		<defName>EmptyCrate_cider</defName>
		<label>empty cider crate</label>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>cider_Crate</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>cider_Crate</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<cider>75</cider>
		</products>
	</RecipeDef>


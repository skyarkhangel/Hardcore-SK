<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef>
		<defName>Make_Patchleather</defName>
		<label>make patchleather</label>
		<description>Combine various types of leather to create weak patchleather.</description>
		<jobString>Making patchleather.</jobString>
		<workSpeedStat>TailoringSpeed</workSpeedStat>
		<effectWorking>Tailor</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<workAmount>900</workAmount>
		<allowMixingIngredients>true</allowMixingIngredients>
		<targetCountAdjustment>50</targetCountAdjustment>
		<recipeUsers>
			<li>LeatherworkerTable</li>
			<li>TableLoom</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Leathers</li>
					</categories>
					<disallowedThingDefs>
						<li>Leather_Thrumbo</li>
					</disallowedThingDefs>
				</filter>
				<count>50</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Leathers</li>
			</categories>
			<disallowedThingDefs>
				<li>Leather_Patch</li>
				<li>Leather_Human</li>
				<li>Chitin</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<products>
			<Leather_Patch>50</Leather_Patch>
		</products>
		<workSkill>Crafting</workSkill>
		<researchPrerequisite>Apparel_A1</researchPrerequisite>
	</RecipeDef>


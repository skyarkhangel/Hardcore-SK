<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!--========================= Animal trader =============================-->


	<TraderKindDef>
		<defName>AnimalTrader</defName>
		<label>Animal Trader</label>
		<commonality>1.2</commonality>
		<orbital>true</orbital>
		<stockGenerators>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>5000~8000</countRange>
			</li>
			<li Class="StockGenerator_Animals">
				<tradeTagsSell>
					<li>AnimalCommon</li>
					<li>AnimalUncommon</li>
					<li>AnimalFighter</li>
					<li>AnimalFarm</li>
					<li>AnimalPet</li>
				</tradeTagsSell>
				<tradeTagsBuy>
					<li>AnimalCommon</li>
					<li>AnimalFighter</li>
					<li>AnimalExotic</li>
				</tradeTagsBuy>
				<kindCountRange>4~7</kindCountRange>
				<countRange>10~20</countRange>
				<createMatingPair>
					<li>AnimalFarm</li>
				</createMatingPair>
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>Leathers</categoryDef>
				<thingDefCountRange>2~8</thingDefCountRange>
				<totalPriceRange>600~2200</totalPriceRange>
			</li>
		</stockGenerators>
	</TraderKindDef>

</Defs>
<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef Name="AlcoholCrateBase" ParentName="SK_ResourceBase" Abstract="True">
		<description>A crate containing various items depending on the crate type. Alcohol crates always contain 75 bottles of the corresponding type.</description>
		<graphicData>
			<graphicClass>Graphic_StackCount</graphicClass>
			<drawSize>(2,2)</drawSize>
		</graphicData>
		<resourceReadoutPriority>First</resourceReadoutPriority>
		<statBases>
			<Beauty>-7</Beauty>
			<MaxHitPoints>75</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<MarketValue>675</MarketValue>
		 	<Mass>25</Mass>
		</statBases>
		<stackLimit>2</stackLimit>
		<thingCategories>
			<li>Alcohol</li>
		</thingCategories>
		<tradeTags>
			<li>Exotic</li>
		</tradeTags>
	</ThingDef>
	
	
	<ThingDef Name="Patreonbrew_base" ParentName="AlcoholCrateBase" Abstract="True">
		<description>A brew of deliciousness from our supporters.</description>
	</ThingDef>	


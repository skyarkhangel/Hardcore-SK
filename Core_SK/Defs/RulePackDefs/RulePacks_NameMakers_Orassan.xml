<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonOrassan</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[oracat] [orabreed]</li>
        <li>name->[maleoras] '[oracat]' [orabreed]</li>
        <li>name->[femaleoras] '[oracat]' [orabreed]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>maleoras</keyword>
          <path>Names/Animal_Male</path>
        </li>

        <li Class="Rule_File">
          <keyword>femaleoras</keyword>
          <path>Names/Animal_Female</path>
        </li>
        
        <li Class="Rule_File">
          <keyword>orabreed</keyword>
          <path>OrassanNames/breed</path>
        </li>
        <li Class="Rule_File">
          <keyword>oracat</keyword>
          <path>OrassanNames/cat</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>